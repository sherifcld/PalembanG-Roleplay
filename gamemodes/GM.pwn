public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
    if(!PlayerInfo[playerid][pLicenses][3]) return ShowInfo(playerid, "Anda tidak memiliki lisensi senjata."), ResetPlayerWeapons(playerid);
	if(PlayerInfo[playerid][pLevel] < 1 && hittype == BULLET_HIT_TYPE_PLAYER)
	{
	    SCM(playerid, COLOR_GREY, "Anda dapat menangani kerusakan dari level 1.");
	    return false;
	}

    if(IsACop(playerid) && form_fraction[playerid] && hittype == BULLET_HIT_TYPE_PLAYER && weaponid == 23) return BulletTazer(playerid, hitid);

    if(hittype == BULLET_HIT_TYPE_PLAYER)
	{
		killer_id[hitid] = playerid;
		killer_weapon[hitid] = weaponid;
		killer_clear[hitid] = 10;
    }

    return 1;
}
public OnPlayerGiveDamage(playerid, damagedid, Float: amount, weaponid, bodypart)
{
    if(damagedid != INVALID_PLAYER_ID)
	{
	    if(PlayerInfo[playerid][pLevel] < 1)
		{
		    new Float: health, Float: armour;

			GetPlayerHealth(damagedid, health);
		    GetPlayerArmour(damagedid, armour);

		    SetPlayerHealth(damagedid, health);
		    SetPlayerArmour(damagedid, armour);

		    SCM(playerid, COLOR_GREY, "Anda dapat menangani kerusakan dari level 1.");
		    return 0;
		}

		killer_id[damagedid] = playerid;
		killer_weapon[damagedid] = weaponid;
		killer_clear[damagedid] = 10;
	}

    return 1;
}
public OnPlayerTakeDamage(playerid, issuerid, Float: amount, weaponid, bodypart)
{
	killer_id[playerid] = issuerid;
	killer_weapon[playerid] = weaponid;

    return 1;
}


CMD:dice(playerid, params[])
{
    if(PlayerInfo[playerid][pLevel] < 2) return SCM(playerid, COLOR_GREY, "Tersedia dari level 2!");

	if(!PlayerToPoint(50.0, playerid, 1996.1062,1165.2214,994.8906) &&
	    !PlayerToPoint(50.0, playerid, 1130.4799,-1.7065,1000.6797)) return SCM(playerid, COLOR_GREY, "Anda tidak di kasino.");

	extract params -> new to_player, money; else return SCM(playerid, 0xCECECEFF, "Gunakan: /dice [id pemain] [jumlahnya]");

	if(!IsPlayerConnected(to_player) || to_player == playerid)
		return SendClientMessage(playerid, 0x999999FF, "Tidak ada pemain seperti itu");

	if(!ProxDetectorS(5.0, playerid, to_player))
		return SendClientMessage(playerid, 0x999999FF, "Pemainnya terlalu jauh");

    if(PlayerInfo[to_player][pLevel] < 2) return SCM(playerid, COLOR_GREY, "Pemain ini kurang dari level 2!");

	if(!(1000 <= money <= 100000))
		return SendClientMessage(playerid, 0x999999FF, "Jumlahnya tidak boleh kurang dari 1000 dan lebih dari $100000");

	if(PlayerInfo[playerid][pCash] < money)
		return SendClientMessage(playerid, 0x999999FF, "Anda tidak punya uang sebanyak itu");

	if(PlayerInfo[to_player][pCash] < money)
		return SendClientMessage(playerid, 0x999999FF, "Pemain tidak memiliki uang sebanyak itu");

	if(GetPlayerOfferInfo(to_player, O_INCOMING_PLAYER) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain sudah memiliki permintaan pertukaran masuk!");

	SendPlayerOffer(playerid, to_player, OFFER_TYPE_DICE, money);

	SCMF(playerid, 0x6BB3FFAA, "Anda menyarankan %s bermain dadu denganmu. Tawaran: $%i", GetPlayerNameEx(to_player), money);
	SCMF(to_player, 0x6BB3FFAA, "%s mengundang Anda untuk bermain dadu. Tawaran: $%i", GetPlayerNameEx(playerid), money);

    return SCM(to_player, -1, "Untuk menyetujui, klik Y(/accept) atau untuk penolakan N(/cancel)");
}

CMD:demote(playerid, params[])
{
	if(PlayerInfo[playerid][pMember] != FRACTION_FBI && PlayerInfo[playerid][pMember] != FRACTION_MAYOR) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");

	if(PlayerInfo[playerid][pRank] < FracInfo[PlayerInfo[playerid][pMember]][fRankMax]-1) return Send(playerid,COLOR_GREY,"Anda bukan pemimpin/wakil organisasi!");

	new to_player, reason[64];

    if(sscanf(params,"us[64]", to_player, reason)) return SCM(playerid, COLOR_GREY, "/demote [id] [alasan]");

    if(PlayerInfo[playerid][pMember] == FRACTION_FBI && !IsACop(to_player) && !IsAArmy(to_player)) return SCM(playerid, COLOR_GREY, "Hanya tersedia di Army dan PD.");

    if(PlayerInfo[playerid][pMember] == FRACTION_MAYOR && !IsACop(to_player) && !IsAArmy(to_player) && PlayerInfo[to_player][pMember] != FRACTION_FBI) return SCM(playerid, COLOR_GREY, "Hanya tersedia di Army, PD dan FBI");

    if(PlayerInfo[to_player][pRank] >= FracInfo[PlayerInfo[to_player][pMember]][fRankMax]-2) return Send(playerid,COLOR_GREY,"Pemain ini memiliki peringkat terlalu tinggi.");

    PlayerInfo[to_player][pRank] --;

	if(PlayerInfo[to_player][pRank] > 0)
	{
	    SCMF(playerid, -1, "Anda diturunkan peringkatnya %s sebelumnya %i. Alasan: %s", GetPlayerNameEx(to_player), PlayerInfo[to_player][pRank], reason);
	    SCMF(to_player, -1, "%s menurunkan Anda ke peringkat %i. Alasan: %s", GN(playerid), PlayerInfo[to_player][pRank], reason);
    }
    else
    {
        PlayerInfo[to_player][pLeader] = 0;
	    PlayerInfo[to_player][pSetSpawn] = 1;
	    PlayerInfo[to_player][pMember] = 0;
	    PlayerInfo[to_player][pRank] = 0;
	    PlayerInfo[to_player][pfWarn] = 0;
		form_fraction[to_player] = 0;

		SavePlayerFraction(to_player);

		SetPlayerSkinEx(to_player, PlayerInfo[to_player][pSkin]);
		SetPlayerToTeamColor(to_player);

		SCMF(playerid, -1, "Anda dipecat dari organisasi %s. Alasan: %s", GetPlayerNameEx(to_player), reason);
	    SCMF(to_player, -1, "%s memecatmu dari organisasi. Alasan: %s", GN(playerid), reason);
    }

    return SavePlayerFraction(to_player);
}


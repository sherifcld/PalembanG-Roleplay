    if(admin_level[playerid] < 2 || !admin_logged[playerid]) return 1;
	if(sscanf(params, "d",params[0])) return Send(playerid, -1, "Masuk: /g(oto) [id]");

	new Float:plocx,Float:plocy,Float:plocz;
	if(!IsPlayerConnected(params[0])) return 1;
	if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return Send(playerid, COLOR_GREY, "Pemain tidak memasuki permainan.!");
	if(GetPlayerState(params[0]) == PLAYER_STATE_SPECTATING && gSpectateID[params[0]] != INVALID_PLAYER_ID) return Send(playerid, COLOR_GREY, "Pemain dalam mode observasi!");

	GetPlayerPos(params[0], plocx, plocy, plocz);
	if(GetPlayerState(playerid) == 2)
	{
		new tmpcar = GetPlayerVehicleID(playerid);
		SetVehiclePos(tmpcar, plocx, plocy+4, plocz+1);
	}
	else SetPlayerPos(playerid,plocx,plocy+2, plocz), SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(params[0])), SetPlayerInterior(playerid, GetPlayerInterior(params[0]));

   // SCMF(params[0], COLOR_GREY, "Administrator teleport ke Anda %s[%d]", Name(playerid), playerid);

	format(format_string, 144, "[A] %s teleport ke pemain %s[%i]", GetPlayerNameEx(playerid), GetPlayerNameEx(params[0]), params[0]);
	SendAdminMessage(COLOR_GREY, format_string);

	return 1;

publics: LoadPlayerContainer(playerid)
{
    DestroyDynamicObject(crane_unload_obj[0]);

    crane_unload_use = INVALID_PLAYER_ID;

    if(!IsPlayerConnected(playerid)) return 0;

	new crane_id = MAX_CRANES;

    for(new i; i < 4; i++)
	{
    	MoveDynamicObject(
			crane_unload_obj[i],
		  	crane_start_pos[crane_id][i][0],
 			crane_start_pos[crane_id][i][1],
			crane_start_pos[crane_id][i][2]-000.4,
			0.4,

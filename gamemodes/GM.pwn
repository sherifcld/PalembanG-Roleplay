	    }
	}

	format(format_string, 188, "INSERT INTO `history_name` (`user_id`,`old_nick`,`new_nick`) VALUES (%i,'%s','%s');", GetPlayerAccountID(playerid), GetPlayerNameEx(playerid), new_nick);
	mysql_tquery(mMysql, format_string);

	SetPlayerName(playerid, new_nick);

	format(PlayerInfo[playerid][pName], MAX_PLAYER_NAME, new_nick);

	return 1;
}

stock GiveFuelMoney(f_id, money, prod = 0)
{
	if(BizzAInfo[f_id][aProducts] > 0)
	{
	    if(BizzAInfo[f_id][aOwned])
	    {
	    	BizzAInfo[f_id][aMoney] += money;
			BizzAInfo[f_id][aRaschet] += money;
		}

		BizzAInfo[f_id][aProducts] -= prod;
	}
}

stock GiveBusinessMoney(b_id, money, prod = 0)
{
    if(BizzInfo[b_id][bMoneyLimit]) return 0;

	if(BizzInfo[b_id][bProducts] > prod)
	{
	    if(BizzInfo[b_id][bOwned])
	    {
	        new bizz_half = (money / 100) * 96;
	        new mafia_half = (money / 100) * 4;

	    	BizzInfo[b_id][bMoney] += bizz_half;
			BizzInfo[b_id][bRaschet] += bizz_half;

			if(BizzInfo[b_id][bMafia])
			{
			    new gang_id = GetGangWHID(BizzInfo[b_id][bMafia]);

			    wh_money_gang[gang_id] += mafia_half;

	            UpdateGangWH3DTextLabel(gang_id);

	            SaveGangWH(gang_id);
			}
		}

		BizzInfo[b_id][bProducts] -= prod;
	}

	if(BizzInfo[b_id][bBuyPrice] < 1000000)
	{
		if(BizzInfo[b_id][bMoney] > 400000) BizzInfo[b_id][bMoney] = 400000, BizzInfo[b_id][bMoneyLimit] = 1;
		if(BizzInfo[b_id][bRaschet] > 400000) BizzInfo[b_id][bRaschet] = 400000;
	}
	else if(1000000 <= BizzInfo[b_id][bBuyPrice] <= 2600000)
	{
		if(BizzInfo[b_id][bMoney] > 500000) BizzInfo[b_id][bMoney] = 500000, BizzInfo[b_id][bMoneyLimit] = 1;
		if(BizzInfo[b_id][bRaschet] > 500000) BizzInfo[b_id][bRaschet] = 500000;
	}
	else
	{
	    if(BizzInfo[b_id][bMoney] > 800000) BizzInfo[b_id][bMoney] = 800000, BizzInfo[b_id][bMoneyLimit] = 1;
		if(BizzInfo[b_id][bRaschet] > 800000) BizzInfo[b_id][bRaschet] = 800000;
	}

	return 1;
}


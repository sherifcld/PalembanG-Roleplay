stock CheckIP(str[])
{
    new index, chislo, i;
    do
    {
        if(str[i] >= '0' && str[i] <= '9') chislo = chislo*10 + (str[i] - '0');
        else
        {
            if(chislo > 100 && chislo < 256 && index < 4) index++;
            chislo = 0;
        }
    }
    while(str[i++] != '\0');
    if(index == 4) return 1;
    return 0;
}


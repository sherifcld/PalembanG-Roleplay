	if(PlayerInfo[playerid][pRank] < FracInfo[PlayerInfo[playerid][pMember]][fRankMax]-1) return Send(playerid,COLOR_GREY,"Anda bukan pemimpin/wakil pemimpin");
	if(PlayerInfo[playerid][pMember] != PlayerInfo[params[0]][pMember]) return Send(playerid,COLOR_GREY,"Pemain tidak ada di organisasi Anda.");
	if(PlayerInfo[playerid][pRank] <= PlayerInfo[params[0]][pRank] || PlayerInfo[params[0]][pLeader] > 0) return Send(playerid,COLOR_GREY,"Anda tidak dapat memecat pemain ini");
	if(params[0] == playerid) return Send(playerid,COLOR_GREY,"Anda tidak dapat mengusir diri sendiri!");

	foreach(new i : capture_zone_list)
	{
	    if(PlayerInfo[playerid][pMember] != GZInfo[i][gFraction] && PlayerInfo[playerid][pMember] != GZInfo[i][gAttack]) continue;

		for(new j; j < 9; j++) TextDrawHideForPlayer(params[0], GZInfo[i][gTD][j]);

		break;
	}

	SCMF(params[0], 0x6BB3FFAA, "%s(%i) memecatmu dari organisasi (%s). Alasan: %s", Name(playerid), playerid, FracInfo[PlayerInfo[playerid][pMember]][fName], params[1]);
    SCMF(playerid, 0x6BB3FFAA, "Anda dipecat %s(%i) dari organisasi (%s). Alasan: %s", Name(params[0]), params[0], FracInfo[PlayerInfo[playerid][pMember]][fName], params[1]);

    PlayerInfo[params[0]][pSetSpawn] = 1;
    PlayerInfo[params[0]][pMember] = 0;
    PlayerInfo[params[0]][pRank] = 0;
    PlayerInfo[params[0]][pfWarn] = 0;
	form_fraction[params[0]] = 0;

	SetPlayerSkinEx(params[0], PlayerInfo[params[0]][pSkin]);
	SetPlayerToTeamColor(params[0]);

    SavePlayerFraction(params[0]);

	return 1;
}
CMD:giverank(playerid,params[])
{
    if(!PlayerInfo[playerid][pMember]) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");
	if(PlayerInfo[playerid][pRank] < FracInfo[PlayerInfo[playerid][pMember]][fRankMax]-2) return Send(playerid,COLOR_GREY,"Anda tidak dapat menggunakan perintah ini.");
	if(sscanf(params, "ud", params[0],params[1])) return Send(playerid, -1,"Masuk: /giverank [playerid] [peringkat]");
    if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");//+
 	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum login.");//+;
	if(afk_time[params[0]] > 2) return Send(playerid, -1,"Pemain ini AFK.");//+
	//if(!form_fraction[playerid] || IsAGang(playerid) || IsAMafia(playerid)) return Send(playerid,COLOR_GREY,"Anda tidak memulai hari kerja!");
	if(PlayerInfo[playerid][pMember] != PlayerInfo[params[0]][pMember]) return Send(playerid,COLOR_GREY,"Pemain tidak ada di organisasi Anda..");
	if(params[1] >= FracInfo[PlayerInfo[playerid][pMember]][fRankMax] || params[1] < 1) return Send(playerid,COLOR_GREY,"Peringkat itu salah.");
	if(params[1] >= PlayerInfo[playerid][pRank]) return Send(playerid,COLOR_GREY,"Anda tidak bisa memberi orang peringkat itu..");
	if(PlayerInfo[playerid][pRank] <= PlayerInfo[params[0]][pRank] || PlayerInfo[params[0]][pLeader] > 0) return Send(playerid,COLOR_GREY,"Anda tidak dapat mengubah peringkat pemain ini.");

	if(params[1] < PlayerInfo[params[0]][pRank]) SCMF(params[0], 0x1CCCE6FF, "%s(%i) menurunkan Anda ke %i peringkat (%s)", Name(playerid), playerid, params[1], FracRank[PlayerInfo[playerid][pMember]][params[1]]);
	if(params[1] > PlayerInfo[params[0]][pRank]) SCMF(params[0], 0x1CCCE6FF, "%s(%i) mempromosikan Anda untuk %i peringkat (%s)", Name(playerid), playerid, params[1], FracRank[PlayerInfo[playerid][pMember]][params[1]]);

	PlayerInfo[params[0]][pRank] = params[1];
	SCMF(playerid,0x1CCCE6FF,"Anda dibesarkan %s ke peringkat %s(%d peringkat)",Name(params[0]), FracRank[PlayerInfo[playerid][pMember]][params[1]],params[1]);

    SavePlayerFraction(params[0]);

	return 1;
}


CMD:usedrugs(playerid, params[])
{
    if(sscanf(params, "i", params[0])) return SCM(playerid,0x00D900AA, "[Petunjuk]: {FFFFFF}/usedrugs [gram(1-6)]");
    if(params[0] < 1 || params[0] > 6) return SCM(playerid,0x00D900AA, "Dimungkinkan untuk menggunakan 1 - 6 gram");
    if(PlayerInfo[playerid][pDrugs] < params[0]) return SCM(playerid,COLOR_GREY,"Anda tidak memiliki jumlah obat yang banyak!");

    if(GetPVarInt(playerid,"Drug_Time")) return SCMF(playerid,COLOR_GREY,"Ini akan memungkinkan untuk digunakan %i detik.", GetPVarInt(playerid,"Drug_Time"));

	new Float: health, Float: armor;

	GetPlayerHealth(playerid, health);
    GetPlayerArmour(playerid, armor);
    
	health += params[0] * 10;
    armor += params[0] * 15;
    if(health > 100.0) health = 100.0;
    if(armor > 100.0) armor = 100.0;

	SetPlayerHealth(playerid, health);
	SetPlayerArmour(playerid, armor);
	
    PlayerInfo[playerid][pDrugs] -= params[0];
	PlayerInfo[playerid][pDrugDep] += params[0];

	GameTextForPlayer(playerid, "~r~euphoria", 4000, 4);
    SetPlayerChatBubble(playerid,"menggunakan obat",COLOR_PURPLE,30.0,10000);
    SetPlayerWeather(playerid, -68);

    SetPVarInt(playerid,"Drug_Time", 120);

    format(format_string, 144, "UPDATE `accounts` SET `pDrugs` = %i, `pDrugDep` = %i WHERE `pID` = %i", PlayerInfo[playerid][pDrugs], PlayerInfo[playerid][pDrugDep], GetPlayerAccountID(playerid));
    mysql_tquery(mMysql, format_string);

    if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) ApplyAnimation(playerid,"SMOKING","M_smk_drag",4.1,0,0,0,0,0,1);
    return SCMF(playerid,0x0099FFAA,"Anda menggunakan narkoba! ( %.0f/160 )", health);
}


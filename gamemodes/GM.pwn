	{
	    DestroyDynamic3DTextLabel(gruz_3d[vehicleid]);
		gruz_3d[vehicleid] = Text3D:0;
	}

    //membersihkan
	foreach(new i: Player)
	{
	    if(vehicleid == rent_vehicle_id[i])
        {
            rent_vehicle_id[i] = INVALID_VEHICLE_ID;
            vehicle_rent_owner[vehicleid] = INVALID_PLAYER_ID;

            DestroyVehicle(vehicleid);

            break;
        }

        if(pTemp[i][pBusID] == vehicleid)
        {
            pTemp[i][pBusID] = 0;
         	player_route_id[i] = 0;
            busPos{i} = 0;
            Send(i, 0xafafafff, "Kendaraan kerja Anda telah ditarik!");

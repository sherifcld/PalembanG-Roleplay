public OnVehicleDamageStatusUpdate(vehicleid, playerid)
{
    new Float: carhp;
    GetVehicleHealth(vehicleid, carhp);

	for(new i; i < MAX_PLAYER_VEHICLES; i++)
	{
	    if(GetPlayerVehicleID(playerid) == player_home_car[playerid][i])
		{
			new panels, doors1, lights1, tires;
			if(player_veh_damage[playerid] == 0)
			{
				if(carhp > 300 && carhp < 700)
				{
					if(CarInfo[playerid][cSost][i] > 0) return CarInfo[playerid][cSost][i] --;
					player_veh_damage[playerid] = 5;
				}
			}
			GetVehicleDamageStatus(vehicleid, panels, doors1, lights1, tires);

			CarInfo[playerid][cDamagePanel][i] = panels;
			CarInfo[playerid][cDamageDoors][i] = doors1;
			CarInfo[playerid][cDamageLights][i] = lights1;
			CarInfo[playerid][cDamageTires][i] = tires;
			CarInfo[playerid][cHeal][i] = carhp;
			return 1;
		}
	}

	if(IsPlayerInAnyVehicle(playerid))
    {
        new v = GetPlayerVehicleID(playerid);

		if(VehInfo[v][vFr] != 0)
		{
	    	if(player_veh_damage[playerid] == 0)
			{
		    	if(carhp > 300 && carhp < 600)
				{
					if(VehInfo[v][vSost] > 0) VehInfo[v][vSost] -= 1;
                    player_veh_damage[playerid] = 5;
				}
			}
		}
	}
	return 1;
}

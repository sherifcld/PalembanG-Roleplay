alias:gethere("gh");
CMD:gethere(playerid, params[])
{
    if(admin_level[playerid] < 3 || !admin_logged[playerid]) return 1;
	if(sscanf(params, "d",params[0])) return Send(playerid, -1, "Masuk: /gethere(/gh) [id]");

	if(!IsPlayerConnected(params[0])) return SCM(playerid, COLOR_GREY, "Pemain tidak terhubung.");

	new Float:plocx,Float:plocy,Float:plocz;
	new inter = GetPlayerInterior(playerid);
	new world = GetPlayerVirtualWorld(playerid);

	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, COLOR_GREY, "Pemain tidak diotorisasi!");
	if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return Send(playerid, COLOR_GREY, "Pemain tidak memasuki permainan!");
	if(afk_time[params[0]] > 5) return Send(playerid, COLOR_GREY, "Pemain ini dalam mode AFK!");
	if(PlayerInfo[params[0]][pPrison] != 0) return Send(playerid, COLOR_GREY, "Pemain ini ada di penjara!");

	GetPlayerPos(playerid, plocx, plocy, plocz);

	if(GetPlayerState(params[0]) == 2)
	{
		new tmpcar = GetPlayerVehicleID(params[0]);
		SetVehiclePos(tmpcar, plocx, plocy+4, plocz+1);
	}
	else SetPlayerPos(params[0],plocx,plocy+2, plocz);
    SetPlayerInterior(params[0], inter);
	SetPlayerVirtualWorld(params[0], world);

	SCMF(params[0], COLOR_GREY, "Administrator %s[%d] teleport kamu ke dirimu sendiri.", Name(playerid), playerid);
	SCMF(playerid, COLOR_GREY, "Pemain %s[%d] diteleportasikan kepada Anda!", Name(params[0]), params[0]);
    //format(String, sizeof(String), "~y~<CMD> ~w~%s[%d] teleported player %s[%d]", Name(playerid), playerid, Name(params[0]), params[0]), AdminChatMessage(String);
	return true;
}


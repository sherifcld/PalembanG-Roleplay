CMD:gquest(playerid)
{
    //if(!(5 <= DayOfWeak() <= 7)) return SCM(playerid, COLOR_GREY, "Waktu untuk menyelesaikan pencarian telah berlalu.");

    new q_id = player_quest_info[playerid][quest_id];
	new q_status = player_quest_info[playerid][quest_status];

	if(q_id == GQUEST_TYPE_END)
	    return SCM(playerid, COLOR_GREY, "Anda telah menyelesaikan semua tugas.");

	if(!IsAGang(playerid))
		return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");

	switch(player_quest_info[playerid][quest_id])
	{
	    case GQUEST_TYPE_CARJACK, GQUEST_TYPE_MONEY:
	    {
	        if(q_status == 1)
	        {
		        new vehicleid = player_quest_info[playerid][quest_vehicle_id];

		        new Float: x, Float: y, Float: z;

                if(GetPlayerVehicleID(playerid) == vehicleid)
				{
				    if(q_id == GQUEST_TYPE_CARJACK) x = 1945.6772, y = -2084.2949, z = 13.5495;
				    if(q_id == GQUEST_TYPE_MONEY) x = 2813.3657, y = -1182.7249, z = 25.2697;
				}
				else GetVehiclePos(vehicleid, x, y, z);

		        SetPlayerGPS(playerid, x, y, z);

				if(q_id == GQUEST_TYPE_CARJACK && GetPlayerVehicleID(playerid) != vehicleid) SetTimerEx("SendPlayerQuestInfoSMS", 2000, false, "ii", playerid, 0);

				return Send(playerid, -1, "Tempat itu ditandai di peta. {ff0000}merah {ffffff}ditandai");
			}
	    }
	    case GQUEST_TYPE_DRUGMARK:
	    {
	        if(q_status == 1)
	        {
	            new rand = player_quest_info[playerid][quest_progress];

	            new Float: x = stash_pos[rand][0],
					Float: y = stash_pos[rand][1],
					Float: z = stash_pos[rand][2];

				SetPlayerGPS(playerid, x, y, z);

				return Send(playerid, -1, "Tempat itu ditandai di peta. {ff0000}merah {ffffff}ditandai");
	        }
	    }
	    case GQUEST_TYPE_MATS:
	    {
	        if(q_status == 1)
	        {
	            new g_id = GetFractionGangID(playerid);

		        new vehicleid = player_quest_info[playerid][quest_vehicle_id];

		        new Float: x, Float: y, Float: z;

                if(GetPlayerVehicleID(playerid) == vehicleid)
				{
					x = quest_fraction_mats[g_id][0],
					y = quest_fraction_mats[g_id][1],
					z = quest_fraction_mats[g_id][2];
				}
				else GetVehiclePos(vehicleid, x, y, z);

		        SetPlayerGPS(playerid, x, y, z);

				SetTimerEx("SendPlayerQuestInfoSMS", 2000, false, "ii", playerid, 0);

				return Send(playerid, -1, "Tempat itu ditandai di peta. {ff0000}merah {ffffff}ditandai");
			}
	    }
	    default: ShowPlayerQuestInfoText(playerid);
	}

	return 1;
}


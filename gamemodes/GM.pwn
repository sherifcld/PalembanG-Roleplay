CMD:alldonate(playerid)
{
	if(strcmp(Name(playerid), PROJECT_FOUNDER, true) != 0) return 1;

    new amount_all;
    new amount_day;

	//

	new Cache: result = mysql_query(mMysql, "SELECT sum(amount) as `count` FROM `donate_log` WHERE 1");

	if(!cache_num_rows()) return SCM(playerid, COLOR_GREY, "Donat benar-benar kosong.");

	cache_get_value_int(0, "count", amount_all);

    cache_delete(result);

    SCMF(playerid, -1, "All time: %i gosok.", amount_all);

	//

	result = mysql_query(mMysql, "SELECT sum(amount) as `count` FROM `donate_log` WHERE UNIX_TIMESTAMP(`date`) > (UNIX_TIMESTAMP(NOW()) - 86400)");

	if(!cache_num_rows()) return SCM(playerid, COLOR_GREY, "Donat dalam 24 jam.");

	cache_get_value_int(0, "count", amount_day);

	cache_delete(result);

	//

	return SCMF(playerid, -1, "Last 24 hour's: %i.", amount_day);
}


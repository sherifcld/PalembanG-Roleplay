stock ProductList(playerid, bool: is_product)
{
	new str[96];

	new count = 0;

	String = "";

	if(is_product)
	{
		for(new i; i < MAX_BIZZ; i++)
		{
		    if(BizzInfo[i][bProducts] > 100) continue;
			if(BizzInfo[i][bType] != 1 && BizzInfo[i][bType] != 2 && BizzInfo[i][bType] != 3 && BizzInfo[i][bType] != 7 && BizzInfo[i][bType] != 9) continue;

			format(str, 96, "%i. %s\t%d Produk\n", i, BizzInfo[i][bMessage], BizzInfo[i][bSklad] - BizzInfo[i][bProducts]);
			strcat(String, str);

			SetPlayerListitemValue(playerid, count++, i);
		}
	}
	else
	{
	    for(new i; i < MAX_aBIZZ; i++)
	    {
	        if(BizzAInfo[i][aProducts] > 100) continue;

			format(str, 96, "%i. %s\t%d Bahan bakar\n", i, BizzAInfo[i][aMessage], BizzAInfo[i][aSklad] - BizzAInfo[i][aProducts]);
			strcat(String, str);

			SetPlayerListitemValue(playerid, count++, i);
	    }
	}

	if(count == 0) return SPD(playerid, 0, DIALOG_STYLE_MSGBOX, "{ffff00}Memesan", ""W"Tidak ada pesanan baru", "Tutup", "");

	return SPD(playerid, 146, DIALOG_STYLE_TABLIST, "Memesan", String, "Pilih", "Keluar");
}


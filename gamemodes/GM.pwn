			}
		}
	}

	return 1;
}

public OnPlayerLeaveDynamicArea(playerid, areaid)
{
    foreach(Player, i)
	{
	    if(GetPVarType(i, "BBArea"))
	    {
	        if(areaid == GetPVarInt(i, "BBArea"))
	        {
	            StopStream(playerid);
	        }
	    }
	}

    if(player_bone_table[playerid] != -1) PlayerLeaveBoneTable(playerid);

	if(areaid == mayor_weather_area) SetPlayerWeather(playerid, weather_id);

    if(graffiti_area[0] <= areaid <= graffiti_area[MAX_GRAFFITI - 1])
	{
	    //new index = _:(areaid - graffiti_area[0]);

		if(!IsAGang(playerid)) return 1;

		if(graffiti_player_progress[playerid] != 0) PlayerStopRestyleGraffiti(playerid);
	}

    for(new i; i < MAX_HOUSES; i++)
	{
    	if(areaid != HouseInfo[i][hAreaEnter]) continue;

	    for(new x = 0; x < 3; x++) PlayerTextDrawHide(playerid, UseLAlt[playerid][x]);

	    break;
 	}

 	if(player_enter_fuel[playerid] != 0)
	{
	    for(new i = 0; i < 3; i++) PlayerTextDrawHide(playerid, UseLAlt[playerid][i]);
	 	return player_enter_fuel[playerid] = 0;
 	}
 	if(player_enter_actors[playerid] != 0) return player_enter_actors[playerid] = 0;
 	new r = GetNearestRadar(areaid);
	if(r != -1)
	{
		if(GetPVarInt(playerid, "v_radar") != 0) DeletePVar(playerid, "v_radar");
	}
	return true;
}

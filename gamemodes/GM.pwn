public OnPlayerDeath(playerid, killerid, reason)
{
    if(killer_id[playerid] != INVALID_PLAYER_ID)
	{
		killerid = killer_id[playerid];
		reason = killer_weapon[playerid];
	}
	
	PlayerLeaveHospitalBed(playerid);

	if(player_fuel_steal[playerid] != 0) RemovePlayerAttachedObject(playerid, 3);

    follow_id[playerid] = INVALID_PLAYER_ID;

	if(player_take_collector[playerid] != -1)
	{
	    RemovePlayerAttachedObject(playerid, 4);
	}

    RemovePlayerAmmoBox(playerid, true);

	if(player_rob_id[playerid] != -1)
	{
	    new rob_id = player_rob_id[playerid];

	    format(format_string, 96, "[ROB]: "W"%s {6495ED}tidak mengatasi tugas itu.", GetPlayerNameEx(playerid));
	    SendMessageToRobPlayers(rob_id, 0x6495EDff, format_string);

    	PlayerStopRobbery(playerid);
	}

    RemovePlayerMask(playerid);

	SetPlayerHealth(playerid, 0.0);

	PlayerLoseDuel(playerid);

	RemovePlayerFromContainer(playerid);

	DestroyDeliveryObject(playerid);

    foreach(Player, i) if(!admin_level[i]) SendDeathMessageToPlayer(i, killerid, playerid, reason);

    if(player_race_reg{playerid}) PlayerLeaveRace(playerid);

	if(killerid != INVALID_PLAYER_ID && player_reg_pb[playerid] != -1 && player_reg_pb[killerid] != -1 && player_reg_pb[playerid] != player_reg_pb[killerid])
	{
		player_kills_pb[killerid]++;

		if(player_reg_pb[killerid] == 0) pb_blue_kills++;
		if(player_reg_pb[killerid] == 1) pb_red_kills++;

		foreach(Player, i)
			if(player_reg_pb[i] != -1)
   				SendDeathMessageToPlayer(i, killerid, playerid, reason);
	}

    player_croupier_job{playerid} = 0;

    player_fuel_steal[playerid] = 0;

	if(killerid != INVALID_PLAYER_ID)
	{
		if(IsACop(killerid) && PlayerInfo[playerid][pWanted])
		{
			new place = 0;

		    if(PlayerInfo[killerid][pMember] == FRACTION_LSPD) place = 1;
			else if(PlayerInfo[killerid][pMember] == FRACTION_FBI) place = 2;
			else if(PlayerInfo[killerid][pMember] == FRACTION_SFPD) place = 3;
			else if(PlayerInfo[killerid][pMember] == FRACTION_LVPD) place = 4;

			SetPlayerPrison(playerid, place, (PlayerInfo[playerid][pWanted]*10)*60);

			SetPlayerWantedLevel(playerid, 0);

			SCMF(killerid, 0xFFFF00FF, "Anda menanam %s[%d] pada %d detik", Name(playerid), playerid, PlayerInfo[playerid][pPrison]);
		    SCMF(playerid, 0xFFFF00FF, "%s[%d] menanam Anda %d detik", Name(killerid), killerid, PlayerInfo[playerid][pPrison]);

			GivePlayerPayCheck(killerid, 300);
			PlayerInfo[playerid][pWanted] = 0;
			PlayerCuffed[playerid] = 0;

			if(follow_id[playerid] != INVALID_PLAYER_ID)
			{
				ClearAnimations(playerid, true);
		        follow_id[playerid] = INVALID_PLAYER_ID;
			}
		}

	    if(IsAArmy(playerid) && player_quest_info[killerid][quest_id] == GQUEST_TYPE_KEYSTEAL && player_quest_info[killerid][quest_status] == 1)
        {
            new Float: x, Float: y, Float: z;

            GetPlayerPos(playerid, x, y, z);

            if(player_quest_info[killerid][quest_pickup_id] != 0) DestroyDynamicPickup(player_quest_info[killerid][quest_pickup_id]);

            player_quest_info[killerid][quest_pickup_id] = CreateDynamicPickup(1581, 23, x, y, z, GetPlayerVirtualWorld(playerid), GetPlayerInterior(playerid), killerid);

            SCM(killerid, COLOR_YELLOW, "Militer menjatuhkan kunci, segera mengambilnya.");
        }

		foreach(new i : capture_zone_list)
		{
			if(!PlayerToKvadrat(playerid,GZInfo[i][gPos][0], GZInfo[i][gPos][1],GZInfo[i][gPos][2],GZInfo[i][gPos][3])) continue;
			if(!PlayerToKvadrat(killerid,GZInfo[i][gPos][0], GZInfo[i][gPos][1],GZInfo[i][gPos][2],GZInfo[i][gPos][3])) continue;

			new gang_killer = PlayerInfo[killerid][pMember];
			new gang_killed = PlayerInfo[playerid][pMember];

            if(gang_killer == GZInfo[i][gFraction] && gang_killed == GZInfo[i][gAttack]) GZInfo[i][gKills][0]++;
			if(gang_killer == GZInfo[i][gAttack] && gang_killed == GZInfo[i][gFraction]) GZInfo[i][gKills][1]++;

			format(format_string, 144, "[G] %s terbunuh %s", GetPlayerNameEx(killerid), GetPlayerNameEx(playerid));
			SendFrac(gang_killer, -1, format_string);
			SendFrac(gang_killed, -1, format_string);

			break;
		}
	}

	if(!IsAArmy(playerid) && PlayerInfo[playerid][pPrison] == 0 && player_reg_pb[playerid] == -1 && duel_offer[playerid] == INVALID_PLAYER_ID)
	{
		PlayerInfo[playerid][pBolnica] = 1;
	}
	
	if(IsAArmy(playerid))
	{
	    PlayerLeaveHospitalBed(playerid);
	}

    if(rent_job_vehicle[playerid] != INVALID_VEHICLE_ID)
	{
		SetVehicleToRespawnEx(rent_job_vehicle[playerid]);
		player_jobvehicle_time[playerid] = 0;
		rent_job_vehicle[playerid] = INVALID_VEHICLE_ID;
	}
	DisablePlayerCheckpoint(playerid);
    DisablePlayerRaceCheckpoint(playerid);
    
    PlayerInfo[playerid][pLastInju] = 0;
	return 1;
}

public OnVehicleSpawn(vehicleid)
{
    if(VehInfo[vehicleid][vStatus] == 1) Fuell[vehicleid] = MAX_VEHICLE_FUEL;


publics: ShowPlayerOfflineStats(playerid, nick[])
{
    new rows;
    cache_get_row_count(rows);

	if(!rows) return SCM(playerid, COLOR_GREY, "Pemain tidak ditemukan.");

	new result_string[64];

	new sql_id, hotel_id, sex, level, exp, leader, member, rang, job, cash, bank, number, vip, models[3], costs[3];
	new drugs, ammo, metall, drug_dep, game_sec, game_afk_sec, game_hour_sec, last_ip[16], reg_ip[16];

	cache_get_value_int(0, "pID", sql_id);
	cache_get_value_int(0, "pSex", sex);
	cache_get_value_int(0, "pHotel", hotel_id);
	cache_get_value_int(0, "pLevel", level);
	cache_get_value_int(0, "pExp", exp);

	cache_get_value_int(0, "pLeader", leader);
	cache_get_value_int(0, "pMember", member);
	cache_get_value_int(0, "pRank", rang);
	cache_get_value_int(0, "pJob", job);

	cache_get_value_int(0, "pCash", cash);
	cache_get_value_int(0, "pBank", bank);
	cache_get_value_int(0, "pNumber", number);

	cache_get_value_int(0, "pDrugs", drugs);
	cache_get_value_int(0, "pPatron", ammo);
	cache_get_value_int(0, "pMetall", metall);
	cache_get_value_int(0, "pDrugDep", drug_dep);

	cache_get_value_int(0, "pVip", vip);

	cache_get_value_int(0, "pInGameSeconds", game_sec);
	cache_get_value_int(0, "pInGameAFKSeconds", game_afk_sec);
	cache_get_value_int(0, "pInGameHourSeconds", game_hour_sec);

	cache_get_value_name(0, "pIp",last_ip);
	cache_get_value_name(0, "pIpReg",reg_ip);

	cache_get_value_name(0, "cModel",result_string);
	sscanf(result_string, "p<,>a<i>[7]", models);

	cache_get_value_name(0, "cCost",result_string);
	sscanf(result_string, "p<,>a<i>[7]", costs);

    new str[324];

	new atext[200], jtext[200];
	if(sex == 1) atext = "Laki-laki", jtext = "Menikah dengan:\t\t\t-\n";
	else atext = "Perempuan", jtext = "Menikah dengan:\t\t\t-\n";

	new htext[10], h_id = GetPlayerHouseBySqlName(nick);
	if(h_id != -1) format(htext, sizeof(htext), "Nomor rumah%d ($%i)", h_id, HouseInfo[h_id][hValue]);
	else if(hotel_id != -1) format(htext, sizeof(htext), "No. hotel%d", hotel_id);
	else htext = "Tunawisma";

	new btext[64], b_id = GetPlayerBizzBySqlName(nick);
	if(b_id == -1) btext = "Tidak";
	else format(btext, sizeof(btext), "%s ($%i)", BizzInfo[b_id][bMessage], BizzInfo[b_id][bMoney]);

	new azctext[64], a_id = GetPlayerABizzBySqlName(nick);
	if(a_id == 0) azctext = "Tidak";
	else format(azctext, sizeof(azctext), "%s ($%i)", BizzAInfo[a_id][aMessage], BizzAInfo[a_id][aMoney]);

	new drank[20];
	switch(level)
	{
 		case 1..2: drank = "Pemula";
 		case 3..5: drank = "Pemula";
	 	case 6..11: drank = "Tentu";
	 	case 12..15: drank = "Pemain reguler";
	 	case 16..20: drank = "Dikuasai";
	 	case 21..25: drank = "Kakek";
	 	default: drank = "Veteran";
	}

	new namejobs[64];
	switch(job)
	{
	    case 1: namejobs= "Pengangkut Makanan/Bahan Bakar";
		case 2: namejobs= "Sopir bus";
		case 3: namejobs= "Pembersih jalan";
		case 4: namejobs= "Pengemudi truk sampah";
		case 5: namejobs= "Kolektor";
		case 6: namejobs= "Pengemudi truk";
		case 7: namejobs= "Pilot";
		case 13: namejobs= "Penangan kontainer";
		case WORK_TYPE_PIZZA: namejobs = "Pria pengiriman pizza";
		case WORK_TYPE_BULLDOZER: namejobs = "Pengemudi buldoser";
		case 70..72: namejobs= "Supir taksi";
		default: namejobs= "Menganggur";
	}

	new vipname[20+1];
	switch(vip)
	{
	    case 1: vipname= "{CD853F}BRONZE VIP";
		case 2: vipname= "{afafaf}SILVER VIP";
		case 3: vipname= "{ffff00}GOLD VIP";
		default: vipname= "Tidak";
	}

	if(leader) drank = "Pemimpin";

	new infofrac[32];
	if(member == 0) infofrac = "-";
	else format(infofrac, sizeof(infofrac), FracInfo[member][fName]);

	new infojob[64];
	if(job == 0 && member == 0) infojob = "Menganggur";
	else if(job != 0) format(infojob, sizeof(infojob), namejobs);
	else if(member != 0) format(infojob, sizeof(infojob), FracRank[member][rang]);

	String = "";
	format(str, sizeof(str), "\
	{ffffff}Nama depan:\t\t\t\t{1A8FA9}%s\n\
	"W"Level:\t\t\t%d (%i dari %i)\n\
	Uang ada di tangan:\t\t%d$\n\
	Uang di bank:\t\t%d$\n\
	Nomor telepon:\t\t%d\n\
	Obat-obatan:\t\t\t%d gr\n\
	Ammo:\t\t\t%d pcs\n\
	Logam:\t\t\t%d kg\n\
	Gender:\t\t\t\t%s\n\
	Kecanduan:\t\t\t%i\n",
	nick, level, exp, (level+1)*4, cash, bank, number, drugs,
	ammo, metall, atext, drug_dep);
	strcat(String,str);

	format(str, sizeof(str), "\
	%s\n\
	Organisasi:\t\t\t%s\n\
	Bekerja / posisi:\t\t%s\n\n\
	Akomodasi:\t\t\t%s\n\
	Perusahaan:\t\t\t%s\n\
    SPBU:\t\t\t\t%s\n\n\
    Status Game:\t\t%s\n\
	Status VIP:\t\t\t%s\n",
 	jtext, infofrac, infojob, htext, btext, azctext, drank, vipname);
	strcat(String,str);

	if(admin_level[playerid])
	{
	    strcat(String, "\nTransportasi:\n\n");

		for(new i; i < MAX_PLAYER_VEHICLES; i++)
		{
		    if(models[i] == 0)
		    {
		        format(str, 48, "%i. Kosong\n", i + 1);
			    strcat(String, str);
		    }
		    else
			{
				format(str, 48, "%i. %s ($%i)\n", i + 1, VehicleNames[models[i] - 400], costs[i]);
			    strcat(String, str);
			}
		}
	}

    format(str, 324,
        "\nDimainkan dalam sehari: %s\n\
		AFK per hari: %s\n\
		Menang tanpa AFK: %s\n\
		Menang dalam satu jam terakhir: %s\n\
		L-IP: %s\n\
		R-IP: %s",
		ConvertSeconds(game_sec),
		ConvertSeconds(game_afk_sec),
		ConvertSeconds(game_sec - game_afk_sec),
		ConvertSeconds(game_hour_sec),
		last_ip,
		reg_ip);

	strcat(String ,str);

	return SPD(playerid,0,DIALOG_STYLE_MSGBOX,"{E5D81E}Statistik pemain resmi",String, "Tutup", "");
}


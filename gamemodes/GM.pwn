CMD:calls(playerid)
{
	if(!IsACop(playerid) && !(70 <= PlayerInfo[playerid][pJob] <= 72)) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");

	new str[64], Float: dist, count;

	new Float: x, Float: y, Float: z;

    GetPlayerPos(playerid, x, y, z);

	String = "";

	foreach(Player, i)
	{
	    if((70 <= PlayerInfo[playerid][pJob] <= 72) && PlayerInfo[playerid][pJob] != player_call_taxi[i]) continue;
		if(IsACop(playerid) && !player_call_police[i]) continue;
		if(IsAMedic(playerid) && !player_call_hospital[i]) continue;

		dist = GetPlayerDistanceFromPoint(i, x, y, z);

		if(dist > 2000.0) continue;

		format(str, 64, "%i\t%s\t%.1f\n", i, GetPlayerNameEx(i), i);
		strcat(String, str);

		SetPlayerListitemValue(playerid, count++, i);
	}

	if(count == 0) return SCM(playerid, COLOR_GREY, "Tidak ada panggilan aktif.");

	return SPD(playerid, 7746, DIALOG_STYLE_TABLIST, "Daftar panggilan", String, "Terima", "Tutup");

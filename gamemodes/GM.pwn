							SendMessageToRobPlayers(rob_id, 0xffff00ff, "SMS: Saya mengirimi Anda koordinat rumah yang harus Anda tembus, pergi ke sana. | Pengirim: Ujang");

							break;
						}

						if(rob_info[rob_id][r_house] == -1)
						{
						    new partnerid = GetPlayerRobPartnerID(playerid);

						    if(partnerid != INVALID_PLAYER_ID) PlayerStopRobbery(partnerid);
						    PlayerStopRobbery(playerid);

						    return SCM(playerid, COLOR_GREY, "Semua rumah yang tersedia sudah dirampok.");
						}
				    }

				    new h_id = rob_info[rob_id][r_house];

					SetPlayerGPS(playerid, HouseInfo[h_id][hEntrancex], HouseInfo[h_id][hEntrancey], HouseInfo[h_id][hEntrancez]);
				}
			}
   		}

   		if(quest_vehicle_owner[vehicleid] != INVALID_PLAYER_ID)
   		{
   		    if(quest_vehicle_owner[vehicleid] != playerid)
   		    {
				SCM(playerid, COLOR_GREY, "Ini adalah kendaraan pencarian, Anda tidak dapat menggunakannya.");
   		        return RemovePlayerFromVehicleEx(playerid);
   		    }

   		    callcmd::gquest(playerid);
   		}

   		//
		if(VehInfo[vehicleid][vStatus] == 0)
		{
		    if(player_quest_info[playerid][quest_id] == GQUEST_TYPE_KEYPDSTEAL && player_quest_info[playerid][quest_status] == 1 && player_quest_info[playerid][quest_progress] == 1 && (VehInfo[playerid][vFr] == FRACTION_LSPD || VehInfo[playerid][vFr] == FRACTION_SFPD || VehInfo[playerid][vFr] == FRACTION_LVPD))
		    {
		        //Kami tidak melakukan apa pun sehingga pemain dapat masuk ke mobil polisi dengan pencarian
		        SCM(playerid, COLOR_YELLOW, "[QUEST] {FFFFFF}Gunakan perintah: /kpk untuk meretas komputer");
		    }

		    else if(VehInfo[vehicleid][vFr] != form_fraction[playerid])
			{
				SCMF(playerid, COLOR_GREY, "Anda bukan anggota organisasi %s atau Anda tidak memulai hari kerja!", FracInfo[VehInfo[vehicleid][vFr]][fName]);
				return RemovePlayerFromVehicleEx(playerid);
			}

			else if(VehInfo[vehicleid][vModel] == 433 && VehInfo[vehicleid][vFr] == 3)
			{
			    if(PlayerInfo[playerid][pRank] < 4) return Send(playerid, COLOR_GREY, "Transportasi tersedia dari peringkat 4."), RemovePlayerFromVehicleEx(playerid);
				//GameTextForPlayer(playerid, "~w~USE ~g~/CARM", 3000, 1);
			}

			if(army_veh_pickup[vehicleid] != 0)
			{
			    SPD(playerid, 8613, DIALOG_STYLE_MSGBOX, "Pemuatan amunisi", "Anda ingin menyelesaikan pemuatan amunisi?", "Ya", "Tidak");
			}
		}
		else if(VehInfo[vehicleid][vStatus] == 67)
		{
		    if(player_road_school[playerid] == 0)
			{
			    RemovePlayerFromVehicleEx(playerid);
				return SendClientMessage(playerid, COLOR_GREY, "Anda tidak lulus ujian mengemudi.");
		    }

		    if(player_road_school[playerid] == 1)
		    {
		        player_road_school[playerid] = 2;
		        player_road_id[playerid] = 0;

		        SetPlayerSchoolPoint(playerid);
		    }
		    else if(player_road_school[playerid] == 2)
		    {
		        player_school_back[playerid] = 0;
		        SendClientMessage(playerid, COLOR_GREEN, "Anda kembali dalam transportasi.");

		        DeletePVar(playerid, "school_vehicle");
		    }
		}
		else if(VehInfo[vehicleid][vStatus] == 1)
		{
		    if(PlayerInfo[playerid][pJob] == VehInfo[vehicleid][vFr])
			{
			    if(rent_job_vehicle[playerid] != vehicleid)
				{
  					if(PlayerInfo[playerid][pJob] == 2 && GetVehicleModel(pTemp[playerid][pBusID]) == 431)
					{
					    new newcar = GetPlayerVehicleID(playerid);
					    if(pTemp[playerid][pBusID] == newcar) {
			                if(pTemp[playerid][bus_timer])pTemp[playerid][bus_timer] = 0;
			                ShowPlayerSpeed(playerid);
							SetNextCP(playerid,busPos{playerid});
							return 1;
			            }
					}
					if(PlayerInfo[playerid][pJob] == 3 && GetVehicleModel(pTemp[playerid][puborkacar]) == 574)
					{
		                if(pTemp[playerid][uborka_timer]) pTemp[playerid][uborka_timer] = 0;
		                ShowPlayerSpeed(playerid);
						SetNextCP(playerid,uborkaPos{playerid});
						return 1;
					}
					if(VehInfo[vehicleid][vFr] == 6 && rent_job_vehicle[playerid] != INVALID_VEHICLE_ID)
					{
					    Send(playerid,-1,!"Anda kembali ke truk!");
					}
					else if(VehInfo[vehicleid][vFr] == 7 && rent_job_vehicle[playerid] != INVALID_VEHICLE_ID)
					{
					    Send(playerid,-1,"Anda kembali ke pesawat");
					}
					else if(VehInfo[vehicleid][vFr] == WORK_TYPE_BULLDOZER)
					{
					    bd_player_state[playerid] = 1;

					    UpdatePlayerBulldozerMark(playerid);
					}
					else SPD(playerid, 149, DIALOG_STYLE_MSGBOX, "Penyewaan mobil", ""W"Yakin ingin menyewa kendaraan yang berfungsi?", "Ya", "Tidak"); // pekerjaan lain\nHarga sewa: $500
				}
				else
				{
					if(VehInfo[vehicleid][vFr] == 4) trash_player_back[playerid] = 0;
					if(VehInfo[vehicleid][vFr] == 5) collector_player_back[playerid] = 0;
					if(VehInfo[vehicleid][vFr] == WORK_TYPE_DELIVERY) UpdatePlayerDeliveryMark(playerid);
					if(VehInfo[vehicleid][vFr] == WORK_TYPE_PIZZA) UpdatePlayerPizzaMark(playerid);
					if(VehInfo[vehicleid][vFr] == WORK_TYPE_BULLDOZER)
					{
						if(bd_veh_obj[vehicleid] != 0) bd_player_state[playerid] = 2;

						UpdatePlayerBulldozerMark(playerid);
					}
				}
				GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
				if(engine == 0) SetVehicleParamsEx(vehicleid,1,lights,alarm,doors,bonnet,boot,objective);
    			/*if(VehInfo[vehicleid][vFr] == 6 && rent_job_vehicle[playerid] != INVALID_VEHICLE_ID)
				{
				    Send(playerid,-1,!"Anda kembali ke truk!");
				}*/
			}
			else
			{
		 		Send(playerid, COLOR_GREY, "Transportasi ini tidak tersedia untuk Anda!");
		 		RemovePlayerFromVehicleEx(playerid);
		 		return 1;
		 	}
		}

		ShowPlayerSpeed(playerid);
	}
	if(newstate == PLAYER_STATE_PASSENGER) // penumpang pemain
	{
	    new vehicleid = GetPlayerVehicleID(playerid);
	    //new driver_id = vehicle_driver[vehicleid];

		if(VehInfo[vehicleid][vStatus] == 1)
        {
            if(VehInfo[vehicleid][vFr] == 2)
            {
	            if(PlayerInfo[playerid][pCash] < 50)
	            {
					format(format_string, sizeof(format_string), "{DD90FF}%s[%d] menunjukkan tiket ke supir bus",Name(playerid),playerid);
					ProxDetector(20.0, playerid, format_string, 0xDD90FFAA,0xDD90FFAA,0xDD90FFAA,0xDD90FFAA,0xDD90FFAA);
					SetPlayerChatBubble(playerid,"menunjukkan tiket ke supir bus",0xDD90FFAA,20.0,10000);
				}
				else GivePlayerMoneyEx(playerid, -50, "perjalanan bus");
			}

			if(70 <= VehInfo[vehicleid][vFr] <= 72) SetPlayerAsClientForTaxi(playerid, vehicleid);
        }
	}
	return 1;
}


stock Convert(number)
{
	new sting[100];
	new hours = 0, mins = 0, secs = 0;
	hours = floatround(number / 3600);
	mins = floatround((number / 60) - (hours * 60));
	secs = floatround(number - ((hours * 3600) + (mins * 60)));
	if(hours > 0) format(sting, 100, "%d:%02dm", hours, mins);
	else format(sting, 100, "%d:%02d", mins, secs);
	return sting;
}

stock ConvertSeconds(time)
{
	new string[20];
	format(string, 20, "%i:%02i:%02i", floatround(time/3600), floatround(time%3600/60), time%3600%60);
	return string;
}


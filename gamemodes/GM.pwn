CMD:sethbe(playerid, params[])
{
    new targetid, HBE;
    if(admin_level[playerid] < 1 || !admin_logged[playerid]) return 1;
    
    if(sscanf(params, "ui", targetid, HBE))
        return Send(playerid, -1, "Gunakan: /sethbe [id] [jumlah]");
    if(!IsPlayerConnected(targetid))
        return Send(playerid, -1, "Tidak ada pemain seperti itu di server.");

    if(!(0 <= HBE <= 100))
        return Send(playerid, -1, "Jumlah HBE dari 0 hingga 100.");

    PlayerInfo[playerid][pLapar] = HBE;
    PlayerInfo[playerid][pHaus] = HBE;
    PlayerInfo[playerid][pKencing] = HBE;
    format(format_string, 96, "[A] Administrator %s mengubah HBE %s.", Name(playerid), Name(targetid));
    SendAdminMessage(COLOR_GREY, format_string);
    return true;
}

CMD:pm(playerid, params[])
{
    static text[128], otherid;
    if(sscanf(params, "us[128]", otherid, text))
        return ShowInfo(playerid, "/pm [playerid/PartOfName] [message]");

    if(otherid == INVALID_PLAYER_ID)
        return ShowError(playerid, "Player yang anda tuju tidak valid.");

    if(otherid == playerid)
        return ShowError(playerid, "Tidak dapan PM diri sendiri.");

    PlayerPlaySound(otherid, 1085, 0.0, 0.0, 0.0);

    KirimPesan(otherid, COLOR_YELLOW, "(( PM from %s (%d): %s ))", GetName(playerid), playerid, text);
    KirimPesan(playerid, COLOR_YELLOW, "(( PM to %s (%d): %s ))", GetName(otherid), otherid, text);

    format(format_string, sizeof(format_string), "[SPY PM] %s (%d) to %s (%d): %s", GetName(playerid), playerid, GetName(otherid), otherid, text);
    SendAdminMessage(COLOR_YELLOW, format_string);
    return 1;
}

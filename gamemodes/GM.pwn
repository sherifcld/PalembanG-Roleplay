CMD:sellmilk(playerid, params[])
{
    new inputsusu;
    if(!IsPlayerInRangeOfPoint(playerid, 2, 54.0360,-1623.3668,10.9446)) return ShowError(playerid, "Anda harus di pasar produck");
    if(sscanf(params, "d", inputsusu)) return ShowInfo(playerid, "/sellmilk input");
    new hargasusu = inputsusu * 1000;
    if(inputsusu < 0 || inputsusu > 100) return ShowInfo(playerid, "Jumlah maximal 0 - 100.");
    if(PlayerInfo[playerid][pSusuSapi] < inputsusu) return ShowError(playerid, "Anda tidak memiliki ayam");
    GivePlayerMoneyEx(playerid, hargasusu, "hasil jual susu");
    PlayerInfo[playerid][pSusuSapi] -= inputsusu;
    SCMF(playerid, -1, "Berhasil menjual susu %d seharga %i", inputsusu, hargasusu);

    ShowInfo(playerid, "Berhasil menjual susu anda");
    KirimPesan(playerid, -1, "{00ff00}INFO: {ffffff}Berhasil Jual Susu");
    return 1;
}


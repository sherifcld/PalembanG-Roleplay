stock PlayerWantLoadGun(playerid, bool: is_load)
{
	if(is_load && !PlayerToPoint(30.0, playerid, 2136.6138,-88.3013,2.8688))
	{
	    SetPlayerGPS(playerid, 2136.6138,-88.3013,2.8688);
		return SCM(playerid, COLOR_GREY, "Anda jauh dari posisi memuat, tempat itu ditandai pada peta.");
	}

	if(!is_load)
	{
		if(!PlayerToPoint(30.0, playerid, 335.2584,1943.1689,17.6406) &&
			!PlayerToPoint(30.0, playerid, -303.6902,1533.2164,75.3594) &&
			!PlayerToPoint(30.0, playerid, 1568.4247,-1694.7701,5.8906) &&
			!PlayerToPoint(30.0, playerid, 1406.1659,-1791.5538,13.5469) &&
			!PlayerToPoint(30.0, playerid, 339.2802,2468.9412,20.2886) && //Angkatan udara
			!PlayerToPoint(30.0, playerid, -1289.4197,374.9855,7.1343) && //Angkatan Laut
			!PlayerToPoint(30.0, playerid, -1606.3179,672.1406,-4.9063) && //SFPD
			!PlayerToPoint(30.0, playerid, 2296.8242,2468.6787,10.8203)) //LVPD
	    return SCM(playerid, COLOR_GREY, "Anda jauh dari tempat bongkar(Polisi, Balai Kota, FBI, Gudang Tentara).");
	}

	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
	    return SCM(playerid, COLOR_GREY, "Anda tidak berada di belakang kursi pengemudi.");

	new vehicleid = GetPlayerVehicleID(playerid);

	new model = GetVehicleModel(vehicleid);

	if(model != 470 && model != 433)
		return SCM(playerid, COLOR_GREY, "Kamu tidak spesial. transportasi untuk memuat PSU.");

	if(army_veh_pickup[vehicleid] != 0)
	    return SCM(playerid, COLOR_GREY, "Pemuatan kendaraan sudah dimulai.");

    new Float: x, Float: y, Float: z, Float: a;

	if(!is_load) SCM(playerid, COLOR_YELLOW, "Untuk mengisi gudang fraksi: Ambil kotak dari transportasi dan letakkan di gudang.");

	GetVehiclePos(vehicleid, x, y, z);
	GetVehicleZAngle(vehicleid, a);

	GetXYInFront(x, y, a, model == 470 ? -3.5 : -5.5);

	army_veh_pickup[vehicleid] = CreateDynamicPickup(19133, 23, x, y, z);

	new limit_amount = GetVehicleModel(vehicleid) == 470 ? 5 : 20;

	format(format_string, 64, "Amunisi dimuat: %i/%i", army_veh_amount[vehicleid], limit_amount);
	army_veh_3d[vehicleid] = CreateDynamic3DTextLabel(format_string, COLOR_WHITE, x, y, z, 15.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, 0, 0);

	RemovePlayerFromVehicleEx(playerid);

	return SCMF(playerid, COLOR_GREEN, "Kamu mulai %s amunisi.", is_load ? ("memuat") : ("menurunkan muatan"));
}


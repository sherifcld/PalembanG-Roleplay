CMD:frang(playerid, params[])
{
	new f_id = GetPlayerData(playerid, pFamily);

	if(f_id == -1) return SCM(playerid, -1, "Anda bukan anggota keluarga!");

	if(GetPlayerData(playerid, pFamilyRang) < 3) return SCM(playerid, COLOR_GREY, "Hanya tersedia untuk kepala keluarga.");

    extract params -> new to_player, string:give_rang[2]; else return SendClientMessage(playerid, 0x999999FF, "Gunakan: /frang [id pemain] [+/-]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	if(GetPlayerData(to_player, pFamily) != GetPlayerData(playerid, pFamily)) return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini tidak ada di keluarga Anda.!");
	if(!ProxDetectorS(5.0, playerid, to_player)) return SendClientMessage(playerid, 0xCECECEFF, "Pemainnya terlalu jauh");

    if(GetPlayerData(to_player, pFamilyRang) >= GetPlayerData(playerid, pFamilyRang)) return SCM(playerid, COLOR_GREY, "Pemain ini lebih tinggi atau sama dengan peringkat Anda!");

	if(give_rang[0] == '+')
	{
        if(GetPlayerData(to_player, pFamilyRang) + 1 == GetPlayerData(playerid, pFamilyRang)) return SCM(playerid, COLOR_GREY, "Tidak dapat meningkatkan ke peringkat Anda!");

		if(GetPlayerData(to_player, pFamilyRang) + 1 == 2)
		{
		    format(format_string, 96, "SELECT `pID` FROM `accounts` WHERE `family` = %i AND `family_rang` = 2", f_id);
		    new Cache: result = mysql_query(mMysql, format_string, true);

			if(cache_num_rows() >= 3) return SCM(playerid, COLOR_GREY, "Tidak mungkin mengambil 2 peringkat lebih dari 3 orang!");

			cache_delete(result);
		}

		AddPlayerData(to_player, pFamilyRang, +, 1);

		SCMF(playerid, 0x3399FFFF, "Anda dibesarkan %s[%i] ke peringkat %i", GetPlayerNameEx(to_player), to_player, GetPlayerData(to_player, pFamilyRang));
		SCMF(to_player, 0x3399FFFF, "%s[%i] meningkatkan Anda ke peringkat %i", GetPlayerNameEx(playerid), playerid, GetPlayerData(to_player, pFamilyRang));
	}
	else
	{
		AddPlayerData(to_player, pFamilyRang, -, 1);

		SCMF(playerid, 0x3399FFFF, "Anda diturunkan peringkatnya %s[%i] ke peringkat %i", GetPlayerNameEx(to_player), to_player, GetPlayerData(to_player, pFamilyRang));
		SCMF(to_player, 0x3399FFFF, "%s[%i] menurunkan Anda ke peringkat %i", GetPlayerNameEx(playerid), playerid, GetPlayerData(to_player, pFamilyRang));
	}

    update_int_mysql(to_player, "family_rang", GetPlayerData(to_player, pFamilyRang));

	return 1;
}

CMD:finvite(playerid, params[])
{
	new f_id = GetPlayerData(playerid, pFamily);

	if(f_id == -1) return SCM(playerid, -1, "Anda bukan anggota keluarga!");

	if(GetPlayerData(playerid, pFamilyRang) < 2) return SCM(playerid, COLOR_GREY, "Tersedia dari peringkat 2!");

    extract params -> new to_player; else return SendClientMessage(playerid, 0x999999FF, "Gunakan: /finvite [id pemain]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	if(GetPlayerData(to_player, pFamily) != -1) return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini sudah menjadi anggota keluarga.");
	if(!ProxDetectorS(10.0, playerid, to_player)) return SendClientMessage(playerid, 0xCECECEFF, "Pemainnya terlalu jauh");

	if(GetPlayerOfferInfo(to_player, O_INCOMING_PLAYER) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain sudah memiliki permintaan pertukaran masuk!");

	SCMF(playerid, 0x3399FFFF, "Anda menyarankan %s[%i] bergabunglah dengan keluarga Anda!", GetPlayerNameEx(to_player), to_player);
	SCMF(to_player, 0x3399FFFF, "%s[%i] mengundang Anda untuk bergabung dengan keluarga {33FF66}''%s''.", GetPlayerNameEx(playerid), playerid, family_info[f_id][f_name]);
	SCM(to_player, -1, "Klik {00CC00}Y atau /accept {FFFFFF}untuk membelinya atau {FF6600}N atau /cancel {FFFFFF}untuk penolakan");

    SendPlayerOffer(playerid, to_player, OFFER_TYPE_FAMILY);

	return 1;
}

CMD:funinvite(playerid, params[])
{
	new f_id = GetPlayerData(playerid, pFamily);

	if(f_id == -1) return SCM(playerid, -1, "Anda bukan anggota keluarga!");
    if(GetPlayerData(playerid, pFamilyRang) < 2) return SCM(playerid, COLOR_GREY, "Tersedia dari peringkat 2!");

    extract params -> new to_player; else return SendClientMessage(playerid, 0x999999FF, "Gunakan: /funinvite [id pemain]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	if(GetPlayerData(to_player, pFamily) != f_id) return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini tidak ada di keluarga Anda.");


publics: StartPlayerDuel(playerid)
{
    new d_offer = duel_offer[playerid];

    player_duel_time[playerid]--;

    if(player_duel_time[playerid] == 0)
    {
        GameTextForPlayer(playerid, "~g~GO", 1200, 4);
        GameTextForPlayer(d_offer, "~g~GO", 1200, 4);

        TogglePlayerControllable(playerid, true);
        TogglePlayerControllable(d_offer, true);

        return true;
    }
    else
    {
        GameTextForPlayer(playerid, IntToString(player_duel_time[playerid]), 1200, 4);
        GameTextForPlayer(d_offer, IntToString(player_duel_time[playerid]), 1200, 4);

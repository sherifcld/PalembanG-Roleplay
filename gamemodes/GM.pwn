CMD:offwarn(playerid, params[])
{
	if(admin_level[playerid] < 4 || !admin_logged[playerid]) return 1;

	new x_warn[30], target_name[24];
	if(sscanf(params, "s[24]S()[30]",target_name,x_warn)) return Send(playerid, -1,"Masuk: /offwarn [nama panggilan] [alasan]");

    if(GetPlayerID(target_name) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain online.");

    if(GetPlayerAdminBySql(target_name)) return SCM(playerid, COLOR_GREY, "Pemain ini adalah admin.");

	//

	mysql_format(mMysql, format_string, 144, "SELECT `pID` FROM `accounts` WHERE `pName` = '%e' LIMIT 1", target_name);
	new Cache: result = mysql_query(mMysql, format_string);

	if(cache_num_rows() == 0) return SCMF(playerid, COLOR_GREY, "Pemain %s tidak ditemukan.", target_name);

	cache_delete(result);

	//

	mysql_format(mMysql, format_string, 144, "SELECT * FROM `banlog` WHERE `nameplayer` = '%e' AND `lockstate` = 1", target_name);
	result = mysql_query(mMysql, format_string);

	if(cache_num_rows()) return SCMF(playerid, COLOR_GREY, "Pemain %s diblokir.", params[0]);

    cache_delete(result);

	//

	new sql_id = GetPlayerSqlIDByName(params[0]);

	format(format_string, 144, "SELECT * FROM `warninfo` WHERE `plID` = %i", sql_id);
	result = mysql_query(mMysql, format_string);

	new rows = cache_num_rows();

	cache_delete(result);

	//

	if(rows + 1 == 3)
	{
	    PlayerInfo[playerid][pAdmBan]++;
	    OnPlayerUpdateAdminPer(playerid, "pAdmBan", PlayerInfo[playerid][pAdmBan]);

    	format(format_string, sizeof(format_string), "[OFFLINE] Administrator %s melarang pemain %s selama 7 hari [3/3] peringatan. Alasan: %s",Name(playerid), params[0], x_warn);
		SendAdminMessage(COLOR_LIGHTRED, format_string);
		AddAdminLog(playerid, format_string);

    	mysql_format(mMysql, MySQLStr, 144, "DELETE FROM `warninfo` WHERE `plID` = %d", sql_id);
		mysql_tquery(mMysql, MySQLStr);

    	BlockAccounts(params[0], x_warn, Name(playerid), gettime() + (86400 * 7));

    	return 1;
	}

    //

    PlayerInfo[playerid][pAdmWarn]++;
    OnPlayerUpdateAdminPer(playerid, "pAdmWarn", PlayerInfo[playerid][pAdmWarn]);

    format(format_string, sizeof(format_string), "[OFFLINE] Administrator %s mengeluarkan peringatan kepada pemain %s [%i/3]. Alasan: %s", Name(playerid), params[0], rows+1, x_warn);
	SendAdminMessage(COLOR_LIGHTRED, format_string);
	AddAdminLog(playerid, format_string);

	format(format_string, 144, "UPDATE `accounts` SET `pLeader` = 0, `pMember` = 0 WHERE `pID` = %i", sql_id);
	mysql_tquery(mMysql, format_string);

	mysql_format(mMysql, MySQLStr, 344, "INSERT INTO `warninfo` (`plID`, `wReasonWarn`, `wAdminWarn`, `wWarnDate`, `wUnWarnDate`) VALUES (%d, '%e', '%e', %d, %d)", sql_id, x_warn, Name(playerid), gettime(), gettime() + (86400 * 7));
	mysql_tquery(mMysql, MySQLStr, "", "");

	return 1;
}



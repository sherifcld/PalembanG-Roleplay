stock isNum(const text[])
{
	new kolvo;
	for(new i, z = strlen(text); i < z; i++)
	{
	    if('0' <= text[i] <= '9')
	    {
	        kolvo++;
			if(kolvo == strlen(text)) return false;
	    }
	}
	return true;

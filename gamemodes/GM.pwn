CMD:veh(playerid, params[])// Buat mobil
{
   	if(admin_level[playerid] < 4 || !admin_logged[playerid]) return 1;
    //new car = GetPlayerVehicleID(playerid);
	if(sscanf(params, "ddd", params[0],params[1],params[2])) return SendClientMessage(playerid, -1, "Masuk: /veh [id mobil] [warna 1] [warna 2]");
	if(params[0] > 611 || params[0] < 400) return SendClientMessage(playerid, -1, "ID transportasi dari 400 ke 611");
	if(params[1] > 255 || params[1] < 0) return SendClientMessage(playerid, -1, "ID warna dari 0 hingga 255");
	if(params[2] > 255 || params[2] < 0) return SendClientMessage(playerid, -1, "ID warna dari 0 hingga 255");
	new Float:X,Float:Y,Float:Z;
	GetPlayerPos(playerid, X,Y,Z);

	new carid = AddStaticVehicle(params[0], X,Y,Z, 0.0, params[1], params[2]);

	VehInfo[carid][vSost] = 100;
	VehInfo[carid][vStatus] = 2;

	Fuell[carid] = MAX_VEHICLE_FUEL;

	PutPlayerInVehicle(playerid, carid, 0);
	SetVehicleParamsEx(carid,1,0,0,0,0,0,0);

	SendClientMessage(playerid, 0x27C400FF, "Transportasi dibuat");
	VehicleCreate[carid] = true;
	return true;
}

	if(sscanf(params, "u", params[0])) return Send(playerid, -1,"Masuk: /cuff [playerid]");
	if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");//+
	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum masuk. ");//+;
	if(afk_time[params[0]] > 2) return Send(playerid, -1,"Pemain ini AFK.");//+
	if(params[0] == playerid) return Send(playerid,COLOR_GREY,"Anda tidak dapat menempatkan diri di borgol.");
	if(!ProxDetectorS(5.0, playerid, params[0])) return Send(playerid,COLOR_GREY,"Pemain harus berada di dekat Anda.");
	if(IsACop(params[0])) return Send(playerid, COLOR_GREY,"Anda tidak dapat memborgol seorang legalis.");
	if(PlayerCuffed[params[0]]) return Send(playerid, COLOR_GREY,"Pemain sudah diborgol");
	if(!PlayerInfo[params[0]][pWanted]) return Send(playerid, COLOR_GREY,"Manusia bukan penjahat");
	if(PlayerInfo[params[0]][pPrison] > 0) return Send(playerid, COLOR_GREY, "Pemain itu ada di penjara");

    TogglePlayerControllable(params[0], false);

	if(GetPVarInt(params[0], "fTazzer"))
	{
		KillTimer(timer_tazer[params[0]]);
		DeletePVar(params[0], "fTazzer");
	}
	SCMF(params[0], 0xEAC700FF, "%s memborgol kamu", Name(playerid));
	SCMF(playerid, 0xEAC700FF, "Anda diborgol terus %s", Name(params[0]));
	GameTextForPlayer(params[0],"~r~cuffed", 5000, 3);
	SetPlayerAttachedObject(params[0], 8, 19418, 6, -0.011000, 0.028000, -0.022000, -15.600012, -33.699977, -81.700035, 0.891999, 1.000000, 1.168000);
	SetPlayerSpecialAction(params[0],SPECIAL_ACTION_CUFFED);
	TogglePlayerControllableEx(params[0], 0);
	PlayerCuffed[params[0]] = 1;
	return 1;
}
CMD:uncuff(playerid, params[])
{
	if(!IsACop(playerid)) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");
	if(sscanf(params, "u",params[0])) return Send(playerid, -1,"Masuk: /uncuff [playerid]");
	if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");//+
	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum login");//+;
	if(afk_time[params[0]] > 2) return Send(playerid, -1,"Pemain ini AFK.");//+
	if(params[0] == playerid) return Send(playerid,COLOR_GREY,"ID Pemain tidak valid");

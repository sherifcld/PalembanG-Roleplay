public OnPlayerCommandPerformed(playerid, cmd[], params[], result, flags)
{
    if(!strcmp(cmd, ".pos")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, ".mcar")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, ".cmds")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, ".ik")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, ".shotrepeater")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, ".pos")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, "dgun")) return KickD(playerid, "Cheat detected...");
    if(!strcmp(cmd, "whack")) return KickD(playerid, "Cheat detected...");

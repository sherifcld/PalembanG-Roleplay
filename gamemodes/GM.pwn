	if(sscanf(params, "is[24]S()[30]",params[0],target_name,x_warn)) return Send(playerid, -1,"Masuk: /offban [hari] [nama panggilan] [alasan]");

    if(params[0] > 30 || params[0] < 1) return Send(playerid, COLOR_GREY, "Anda tidak dapat menentukan nilai lebih dari 30 atau kurang dari 1.");

    if(GetPlayerID(target_name) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain sedang online.");

    if(GetPlayerAdminBySql(target_name)) return SCM(playerid, COLOR_GREY, "Pemain ini adalah admin.");

	//

	mysql_format(mMysql, format_string, 144, "SELECT `pID` FROM `accounts` WHERE `pName` = '%e' LIMIT 1", target_name);
	new Cache: result = mysql_query(mMysql, format_string);

	if(cache_num_rows() == 0) return SCMF(playerid, COLOR_GREY, "Pemain% s tidak ditemukan.", target_name);

	cache_delete(result);

	//

	mysql_format(mMysql, format_string, 144, "SELECT * FROM `banlog` WHERE `nameplayer` = '%e' AND `lockstate` = '1'", target_name);
	result = mysql_query(mMysql, format_string);

	if(cache_num_rows()) return SCMF(playerid, COLOR_GREY, "Pemain %s diblokir.", params[1]);

    cache_delete(result);

    //

	new days[20];
	switch(params[1])
	{
	    case 1: days = "hari";
	    case 2..4: days = "hari ini";
		default: days = "hari";
	}

	PlayerInfo[playerid][pAdmBan]++;
    OnPlayerUpdateAdminPer(playerid, "pAdmBan", PlayerInfo[playerid][pAdmBan]);

	format(format_string, sizeof(format_string), "[OFFLINE] pada %s melarang pemain %s pada %d %s. Alasan: %s",Name(playerid), params[1], params[0], days, x_warn);
	SendAdminMessage(COLOR_LIGHTRED, format_string);
	AddAdminLog(playerid, format_string);

	BlockAccounts(params[1], x_warn, Name(playerid), gettime() + (86400 * params[0]));

	return 1;
}

CMD:unban(playerid, params[])
{
    if(admin_level[playerid] < 4 || !admin_logged[playerid]) return 1;
    if(sscanf(params, "s[64]", params[0])) return Send(playerid,-1, "Masuk: /unban [Nick_Player]");

    mysql_format(mMysql, MySQLStr, 144, "SELECT * FROM `banlog` WHERE `nameplayer` = '%e' AND `lockstate` = '1' LIMIT 0,1", params[0]);
	mysql_tquery(mMysql, MySQLStr, "OnPlayerCheckUnBanAcc", "is", playerid, params[0]);
	return 1;
}


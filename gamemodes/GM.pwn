epublic: UpdatePlayerDonate(playerid)
{
	new rows;
    cache_get_row_count(rows);
    if(!rows) return 0;

    new donate;

    cache_get_value_name_int(0, "pRobleCheck", donate);

    if(donate == 0) return 1;

    format(String, 144, "INSERT INTO `donate_log` (`user_id`,`amount`) VALUES (%i,%i)", GetPlayerAccountID(playerid), donate);
	mysql_tquery(mMysql, String);

	if(bonus_donate) donate *= 2;

	PlayerInfo[playerid][pRouble] += donate;

	SCMF(playerid, COLOR_GREY, "Pengisian baru: %i.", donate);

    format(String, 96, "UPDATE `accounts` SET `pRobleCheck` = 0 WHERE `pID` = %i", PlayerInfo[playerid][pID]);
    mysql_tquery(mMysql, String);
	return 1;
}


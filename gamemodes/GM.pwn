epublic: FillCars(playerid,finbenz,benz)
{
	new b = player_enter_fuel[playerid];
	if(b == 0) return TogglePlayerControllableEx(playerid, true), KillTimer(FillTimer[playerid]), SetPVarInt(playerid, "FillSost", 0);

	Fuell[GetPlayerVehicleID(playerid)] += 5.0;
	if(finbenz <= Fuell[GetPlayerVehicleID(playerid)])
	{
		Fuell[GetPlayerVehicleID(playerid)] = finbenz;

		GivePlayerMoneyEx(playerid, -(BizzAInfo[b][aPrice]*benz), "mesin pengisian bahan bakar");

		GiveFuelMoney(b, BizzAInfo[b][aPrice]*benz, benz / 4);

		KillTimer(FillTimer[playerid]);
		TogglePlayerControllableEx(playerid, true);
		SetPVarInt(playerid, "FillSost", 0);
		PlayerPlaySoundEx(0, BizzAInfo[b][aAreaFillX], BizzAInfo[b][aAreaFillY], BizzAInfo[b][aAreaFillZ]);
		SCMF(playerid, -1, " Anda telah berhasil mengisi bahan bakar transportasi Anda pada %d liter per %d$", benz, BizzAInfo[b][aPrice]*benz);
	}
	return true;
}

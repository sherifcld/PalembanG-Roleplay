CMD:sms(playerid, params[])
{
	if(PlayerInfo[playerid][pMuted] > 0) return PlayerMuted(playerid);
	if(PlayerInfo[playerid][pProducts][0] == 0) return Send(playerid, COLOR_GREY, "Anda tidak memiliki ponsel.");
	if(PlayerInfo[playerid][pNumber] == 0) return Send(playerid, COLOR_GREY, "Anda tidak memiliki kartu SIM.");
	if(PhoneOnline[playerid] != 0) return Send(playerid,COLOR_GREY, "Ponsel Anda mati.");
	if(PlayerInfo[playerid][pSummaNumber] <= 0) return Send(playerid, COLOR_GREY, "Anda tidak punya uang di akun ponsel Anda.");
	if(player_gag[playerid]) return SCM(playerid, COLOR_GREY, "Anda mendapat lelucon, Anda tidak bisa bicara.") & 0;

	if(sscanf(params, "ds[80]", params[0], params[1])) return Send(playerid, -1,"Masuk: /sms [angka] [teks]");
	//
    if(params[0] < 0 || params[0] > 999999) return Send(playerid, COLOR_GREY,"Nomor ponsel yang ditentukan tidak valid.");
    if(params[1] == PlayerInfo[playerid][pNumber]) return Send(playerid, COLOR_GREY, "Anda tidak dapat mengirim pesan kepada diri sendiri.");
    new full = 0;

    foreach(new i : Player)
	{
        if(params[0] == PlayerInfo[i][pNumber] && params[0] != 0)
		{
            if(PhoneOnline[i] != 0) return Send(playerid, COLOR_GREY, "Telepon pemanggil mati.");

            full++;
            PlayerPlaySound(i, 1052, 0.0, 0.0, 0.0), PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
            SetPlayerChatBubble(playerid, "SMS >>" ,COLOR_YELLOW,10.0,5000), SetPlayerChatBubble(i, "SMS <<",COLOR_YELLOW,10.0,5000);

            SCMF(i, COLOR_YELLOW, "SMS: %s. Pengirim: %s(%i)[Telp. %d]", params[1], Name(playerid), playerid, PlayerInfo[playerid][pNumber]);
            SCMF(playerid, COLOR_YELLOW, "SMS: %s. Penerima: %s(%i)[Telp. %d]", params[1], Name(i), i, PlayerInfo[i][pNumber]);

            foreach(new x : admin_list) if(player_admin_sms[x]) SCMF(x, COLOR_YELLOW, "[A][SMS] %s[%i] -> %s[%i]: %s", GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(i), i, params[1]);

            if(afk_time[i] > 5) Send(playerid, COLOR_GREY, "Pesan berhasil terkirim. Pemutar SMS dalam AFK.");

			PlayerInfo[playerid][pSummaNumber] -= strlen(params[1])*2/2;
			format(String, 18, "~y~-%d$", strlen(params[1])*2/2), GameTextForPlayer(playerid, String, 1500, 1), GameTextForPlayer(i, "~y~SMS", 1500, 1);
			break;
   		}
	}
	if(full == 0) return Send(playerid, COLOR_GREY,"Nomor ini tidak ada atau pemilik nomor sedang luring.");
  	return 1;
}

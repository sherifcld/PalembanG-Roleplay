CMD:dvehall(playerid)
{
    if(admin_level[playerid] < 4 || !admin_logged[playerid]) return 1;

    new count = 0;

    for(new i; i < MAX_VEHICLES; i++)
    {
		if(!IsValidVehicle(i)) continue;

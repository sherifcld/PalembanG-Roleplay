CMD:robbery(playerid)
{
    if(PlayerInfo[playerid][pMember] != 7) return SendClientMessage(playerid, COLOR_GREY, "Anda tidak dapat menggunakannya.");

	if(player_fuel_steal[playerid] == 100) return SendClientMessage(playerid, COLOR_GREY, "Anda tidak dapat mengambil lebih banyak bahan bakar.");

	if(steal_timer[playerid] != -1) return SendClientMessage(playerid, COLOR_GREY, "Anda sudah merampok sebuah pompa bensin.");

	for(new i; i < MAX_aBIZZ; i++)
	{
	    if(GetPlayerDistanceFromPoint(playerid, BizzAInfo[i][aAreaFillX], BizzAInfo[i][aAreaFillY], BizzAInfo[i][aAreaFillZ]) > 3.5) continue;
	    if(BizzAInfo[i][aGangAttack] != PlayerInfo[playerid][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Geng Anda tidak menyerang pompa bensin ini.");
	    if(BizzAInfo[i][aGangAttackState] != 2) return SendClientMessage(playerid, COLOR_GREY, "Waktu persiapan belum berakhir.");

	    steal_timer[playerid] = -1;

		PlayerStealFuelFromStation(playerid, i);

		SetPlayerAttachedObject(playerid, 3, 1650, 6, 0.1179, 0.0150, 0.0719, 0.0000, -102.2999, 0.0999, 1.0000, 1.0000, 1.0000);

		return SendClientMessage(playerid, COLOR_GREEN, "Anda mulai banyak gas dari pompa bensin!");
	}

	return SendClientMessage(playerid, COLOR_GREY, "Tidak ada pompa bensin di dekat Anda.");
}


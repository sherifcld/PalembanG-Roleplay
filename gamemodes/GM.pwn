         	player_route_id[i] = 0;
            uborkaPos{i} = 0;
            Send(i, 0xafafafff, "Kendaraan kerja Anda telah ditarik!");
            break;
        }
	}
	SetVehicleParamsEx(vehicleid,0,0,0,0,0,0,0);
	return 1;

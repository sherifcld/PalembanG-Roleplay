  			SedangHauling[playerid] = 4;
     		SetPlayerRaceCheckpoint(playerid, 1, 2101.257812,-2045.435546,13.210839,13.210839,13.210839,13.210839, 10.0);
       		return 1;
		}
		else if(SedangHauling[playerid] == 4)

stock SetNextCP(playerid,pos)
{
	if(pos < 0) return false;
	new type = 0;
	if(PlayerInfo[playerid][pJob] == 2)
	{
	    new route = player_route_id[playerid];
		GivePlayerPayCheck(playerid, 15);
		if(gBusCPs[route][pos][0] == gBusCPs[route][pos+1][0] && gBusCPs[route][pos][1] == gBusCPs[route][pos+1][1]) type = 1;
		SetPlayerRaceCheckpoint(playerid,type,gBusCPs[route][pos][0],gBusCPs[route][pos][1],gBusCPs[route][pos][2],gBusCPs[route][pos+1][0],gBusCPs[route][pos+1][1],gBusCPs[route][pos+1][2],4.0);
	}
	else if(PlayerInfo[playerid][pJob] == 3) {
	    new route = player_route_id[playerid];
		GivePlayerPayCheck(playerid, 20);
		if(gUborkaCPs[route][pos][0] == gUborkaCPs[route][pos+1][0] && gUborkaCPs[route][pos][1] == gUborkaCPs[route][pos+1][1]) type = 1;
		SetPlayerRaceCheckpoint(playerid,type,gUborkaCPs[route][pos][0],gUborkaCPs[route][pos][1],gUborkaCPs[route][pos][2],gUborkaCPs[route][pos+1][0],gUborkaCPs[route][pos+1][1],gUborkaCPs[route][pos+1][2],4.0);
	}
	return true;
}

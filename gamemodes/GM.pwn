stock hashit(salt[],password[])
{
    new step3[65],step4[33];
    format(step3,sizeof(step3),"%s%s",MD5_Hash(salt),MD5_Hash(password));

	new i = -1;
	while(step3[++i]) step3[i] = tolower(step3[i]);

    format(step4,sizeof(step4),"%s",MD5_Hash(step3));

	new z = -1;
	while(step4[++z]) step4[z] = tolower(step4[z]);
    return step4;
}

stock IntToString(integer)
{
	new string[16];

	format(string, sizeof string, "%d", integer);

	return string;
}

stock PlayerLoseDuel(playerid)
{
    new d_offer = duel_offer[playerid];
    new cost = player_duel_cost[playerid];

	if(d_offer == INVALID_PLAYER_ID) return 0;

	if(player_duel_start[playerid] == true)
	{
	    SCMF(playerid, -1, "Anda kehilangan duel ke pemain %s", GetPlayerNameEx(d_offer));
	    SCMF(d_offer, -1, "Anda memenangkan duel melawan %s", GetPlayerNameEx(playerid));

	    GivePlayerMoneyEx(playerid, -cost, "tersesat dalam duel");
	    GivePlayerMoneyEx(d_offer, cost, "memenangkan duel");

        ResetPlayerWeapons(playerid);
		ResetPlayerWeapons(d_offer);

	    SetPlayerHealth(d_offer, 100.0);

	    SetPlayerPos(d_offer, 2405.4067,-1470.8069,23.9574);
	    SetPlayerVirtualWorld(d_offer, 0);

	    SetPVarInt(playerid, "spawn_after_duel", 1);
	}

	duel_offer[playerid] = duel_offer[d_offer] = INVALID_PLAYER_ID;
    player_duel_start[playerid] = player_duel_start[d_offer] = false;

    return 1;
}

publics: LoadGraffiti()
{
    new rows;
	cache_get_row_count(rows);

	if(!rows) return 0;

    for(new i = 0; i < MAX_GRAFFITI; i++)
	{
	    cache_get_value_name_int(i, "owner_frac", graffiti_fraction[i]);
	    cache_get_value_name(i, "color_owner", graffiti_colored[i], MAX_PLAYER_NAME);
	    cache_get_value_name_int(i, "block", graffiti_block[i]);

	    graffiti_objects[i] = CreateDynamicObject(19353,graffiti_obj_pos[i][0],graffiti_obj_pos[i][1],graffiti_obj_pos[i][2],0.000,0.000,graffiti_obj_pos[i][3]);

	    graffiti_area[i] = CreateDynamicSphere(graffiti_obj_pos[i][0],graffiti_obj_pos[i][1],graffiti_obj_pos[i][2], 2.0);

		UpdateGraffitiGang(i);
	}

	return print("Grafiti dimuat");
}


CMD:arrest(playerid, params[])
{
    if(!IsACop(playerid)) return 1;
	if(sscanf(params, "u",params[0],params[1])) return Send(playerid, -1,"Masuk: /arrest [playerid]");
	if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");
	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum login");//+;
	if(!ProxDetectorS(4.0, playerid, params[0])) return Send(playerid, COLOR_GREY,"Pemain tidak di dekat Anda");
	if(PlayerInfo[params[0]][pWanted] < 1) return Send(playerid, COLOR_GREY,"Orang ini tidak diinginkan");

	new place = 0;
	new time = (PlayerInfo[params[0]][pWanted]*10)*60;

	if(PlayerCuffed[params[0]])
	{
		GameTextForPlayer(params[0],"~g~uncuffed", 5000, 3);
		if(IsPlayerAttachedObjectSlotUsed(params[0], 8)) RemovePlayerAttachedObject(params[0], 8);
		SetPlayerSpecialAction(params[0], 0);
		PlayerCuffed[params[0]] = 0;
	}
	if(PlayerToPoint(25.0, playerid, 681.9285,2541.6382,-94.6045) && GetPlayerInterior(playerid) == 6 && GetPlayerVirtualWorld(playerid) == 1) //LSPD
	{
		SetPPos(params[0], 686.6003,2552.3213,-94.6045, 0.0, 6, 1, false);
		place = 1;
	}
	else if(PlayerToPoint(15.0, playerid, 200.3945,158.2982,1003.0234) && GetPlayerInterior(playerid) == 3 && GetPlayerVirtualWorld(playerid) == 2) // FBI
	{
		SetPPos(params[0],198.4545,161.8394,1003.0300, 0.0, 3, 2, false);
		place = 2;
	}
	else if(PlayerToPoint(15.0, playerid, 217.0850,120.8270,999.0156) && GetPlayerInterior(playerid) == 10 && GetPlayerVirtualWorld(playerid) == 2) //SFPD
	{
		SetPPos(params[0], 219.2213,109.6325,999.0156,100.2528, 10, 2, false);
		place = 3;
	}
	else if(PlayerToPoint(15.0, playerid, 200.2648,158.2574,1003.0234) && GetPlayerInterior(playerid) == 3 && GetPlayerVirtualWorld(playerid) == 8) //LVPD
	{
		SetPPos(params[0], 198.4545,161.8394,1003.0300, 0.0, 3, 8, false);
		place = 4;
	}
	else return Send(playerid, COLOR_GREY,"Anda harus berada di dekat sel-sel penahanan.");

	SetPlayerPrison(params[0], place, time);

	ResetWeapons(params[0]);
	TogglePlayerControllableEx(params[0], 1);

	SCMF(playerid, 0xFFFF00FF, "Anda menanam %s[%d] pada %d detik", Name(params[0]), params[0], time);
    SCMF(params[0], 0xFFFF00FF, "%s[%d] menanam Anda %d detik", Name(playerid), playerid, time);
	GivePlayerPayCheck(playerid, 300);
	PlayerInfo[params[0]][pWanted] = 0;
	PlayerCuffed[params[0]] = 0;
	if(follow_id[params[0]] != INVALID_PLAYER_ID)
	{
		ClearAnimations(params[0], true);
        follow_id[params[0]] = INVALID_PLAYER_ID;
	}
	MeAction(playerid,"memborgol seorang tersangka");
	MeAction(playerid,"mengirim penjahat ke bullpen");
	return 1;
}

CMD:offmute(playerid, params[])
{
	if(admin_level[playerid] < 4 || !admin_logged[playerid]) return 1;
	if(sscanf(params, "is[32]",params[0],params[1])) return Send(playerid, -1,"Masuk: /offmute [menit] [nama panggilan]");

    if(params[0] > 180 || params[0] < 10) return Send(playerid, COLOR_GREY, "Anda tidak dapat menentukan nilai lebih dari 180 menit atau kurang dari 10 menit.");

    if(GetPlayerID(params[1]) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain online.");

    if(GetPlayerAdminBySql(params[1])) return SCM(playerid, COLOR_GREY, "Pemain ini adalah admin.");

	mysql_format(mMysql, format_string, 144, "SELECT `pID`, `pMuted` FROM `accounts` WHERE `pName` = '%e' LIMIT 1", params[1]);
	new Cache: result = mysql_query(mMysql, format_string);

	if(cache_num_rows() == 0) return SCMF(playerid, COLOR_GREY, "Pemain %s tidak ditemukan.", params[1]);

    new seconds, sql_id;
	cache_get_value_name_int(0, "pID", sql_id);
    cache_get_value_name_int(0, "pMuted", seconds);

	cache_delete(result);

	if(seconds) return SCM(playerid, COLOR_GREY, "Pemain sudah memiliki mut.");

    PlayerInfo[playerid][pAdmMute]++;
    OnPlayerUpdateAdminPer(playerid, "pAdmMute", PlayerInfo[playerid][pAdmMute]);

    format(format_string, 144, "Administrator %s memberi Anda larangan ngobrol di %i min.", GetPlayerNameEx(playerid), params[0]);
    AddPlayerNotifyBySQL(sql_id, format_string);

	format(format_string, 144, "[A][OFFLINE] Administrator %s[%d] mengeluarkan larangan ngobrol %s pada %d menit.", Name(playerid), playerid, params[1], params[0]);
	SendAdminMessage(COLOR_GREY, format_string);
	AddAdminLog(playerid, format_string);

	format(format_string, 144, "UPDATE `accounts` SET `pMuted` = %i WHERE `pName` = '%s'", params[0] * 60, params[1]);
	mysql_tquery(mMysql, format_string);

	return 1;
}


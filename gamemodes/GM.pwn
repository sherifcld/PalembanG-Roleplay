CMD:sellsim(playerid, params[])
{
	if(!PlayerInfo[playerid][pNumber]) return SCM(playerid, COLOR_GREY, "Anda tidak memiliki kartu SIM.");

    new i = player_enter_biz[playerid];
	if(!PlayerToPoint(15.0, playerid,BizzInfo[i][bExitX], BizzInfo[i][bExitY], BizzInfo[i][bExitZ]) || BizzInfo[i][bType] != 3)
		return SCM(playerid, COLOR_GREY, "Anda tidak di toko 24/7!");

	new to_player, price;

    if(sscanf(params,"ui",to_player, price)) return Send(playerid,0xafafafff,"Gunakan: /sellsim [playerid] [price]");

    if(!(1 <= price <= 5000000)) return SCM(playerid, COLOR_GREY, "Jumlah yang tersedia dari $1 hingga $5.000.000");

    if(playerid == to_player)
	    return SCM(playerid, COLOR_GREY, "Anda menunjukkan diri Anda sendiri.");

	if(!ProxDetectorS(5.0, playerid, to_player))
	    return SCM(playerid, COLOR_GREY, "Anda jauh dari pemain ini.");

	if(GetPlayerOfferInfo(to_player, O_INCOMING_PLAYER) != INVALID_PLAYER_ID) return SCM(playerid, COLOR_GREY, "Pemain sudah memiliki permintaan pertukaran masuk!");

	SendPlayerOffer(playerid, to_player, OFFER_TYPE_SELLSIM, price);

	SCMF(playerid, -1, "Anda menyarankan %s beli nomor kamu t. %i. Harga: $%i", GetPlayerNameEx(to_player), PlayerInfo[playerid][pNumber], price);

	SCMF(to_player, -1, "%s menyarankan (a) Anda membeli dari dia nomor t. %i. Harga: $%i", GetPlayerNameEx(playerid), PlayerInfo[playerid][pNumber], price);
    return SCM(to_player, -1, "Untuk menyetujui, klik Y(/accept) atau untuk penolakan N(/cancel)");
}


CMD:sellnasgor(playerid, params[])
{
    if(!IsAPedagang(playerid)) return ShowError(playerid, "Anda Bukan Pedagang");
    if(sscanf(params, "udd", params[0], params[1], params[2])) return Usage(playerid, "/SELLBURGER [PLAYERID] [HARGA] [JUMLAH]");
    if(!ProxDetectorS(4.0, playerid, params[0])) return ShowError(playerid,"Pemain terlalu jauh");
    if(params[0] == playerid) return ShowError(playerid, "Tidak bisa digunakan sendiri");
    
    new targetid = params[0];
    new harga = params[1];
    new Jumlah = params[2];
    
    if(PlayerInfo[playerid][pBurger] == Jumlah) return ShowError(playerid, "Anda Tidak Memiliki Burger");
    PlayerInfo[playerid][pBurger] -= Jumlah;
    PlayerInfo[targetid][pBurger] += Jumlah;
    GivePlayerMoneyEx(targetid, -harga, "Beli Makan");
    GivePlayerMoneyEx(playerid, harga, "Beli Makan");
    
    ShowItemBox(playerid, "Nasgor", "USE_1x", 2663, 3);
    ShowItemBox(targetid, "Nasgor", "ADD_1x", 2663, 3);
    return 1;
}


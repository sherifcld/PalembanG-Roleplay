        PlayerInfo[playerid][pKencing] = 100;
    }
    if(PlayerInfo[playerid][pKencing] < 0)
    {
        PlayerInfo[playerid][pKencing] = 0;
    }
    if(PlayerInfo[playerid][pHP] > 101)

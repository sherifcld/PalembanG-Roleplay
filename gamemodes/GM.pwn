			return 1;
		}
	}
	if(PlayerInfo[playerid][pCheckGoogle] == 2 && GetPVarInt(playerid, "AllowPutCode") == 0)
	{
 		SetPlayerInterior(playerid, 0);
		SetPlayerFacingAngle(playerid, 179.5547);
		SetPlayerCameraPos(playerid, -1817.0808, 1112.9937, 235.4244);
		SetPlayerCameraLookAt(playerid, -1816.5471, 949.6590, 235.4244);
		TogglePlayerControllableEx(playerid, 0);

		SPD(playerid,137,DIALOG_STYLE_INPUT,"{CC9900}Google Authenticator", ""W"Sistem keamanan meminta kode otentikasi.\n\nBuka aplikasi 'Google Authenticator' pada ponsel cerdas Anda dan masukkan kode\nIngatlah bahwa waktu di smartphone harus di Indonesia...", "Login", "Keluar");
		return 1;
	}

    format(MySQLStr, 188, "UPDATE `accounts` SET `pIp` = '%s', `pLogin` = 1 WHERE `pID` = %d", GetPlayerIpEx(playerid), PlayerInfo[playerid][pID]);
	mysql_tquery(mMysql, MySQLStr);

	PlayerInfo[playerid][pLogin] = 1;
	//

	format(MySQLStr, 128, "SELECT * FROM `notifications` WHERE `user_id` = %i", GetPlayerAccountID(playerid));
	mysql_tquery(mMysql, MySQLStr, "ShowPlayerNofitications", "d", playerid);

	format(MySQLStr, 128, "SELECT * FROM `supports` WHERE `user_id` = %i LIMIT 1", GetPlayerAccountID(playerid));
	mysql_tquery(mMysql, MySQLStr, "OnPlayerSupport", "d", playerid);

	format(MySQLStr, 128, "SELECT * FROM `admin` WHERE `Name` = '%s' LIMIT 1", Name(playerid));
	mysql_tquery(mMysql, MySQLStr, "OnPlayerAdminAccounts", "d", playerid);

	if(GetPlayerHouse(playerid) != -1) { if(HouseInfo[GetPlayerHouse(playerid)][hOplata] <= 3 && HouseInfo[GetPlayerHouse(playerid)][hSlet] == 0) Send(playerid, 0xFFE569FF, "Anda memiliki beberapa hari gaji tersisa untuk rumah."); }
	if(GetPlayerBizz(playerid) != -1) { if(BizzInfo[GetPlayerBizz(playerid)][bOplata] <= 3 && BizzInfo[GetPlayerBizz(playerid)][bSlet] == 0) Send(playerid, 0xFFE569FF, "Anda memiliki beberapa hari pembayaran tersisa untuk bisnis ini."); }
	if(GetPlayerABizz(playerid)) { if(BizzAInfo[GetPlayerABizz(playerid)][aOplata] <= 2 && BizzAInfo[GetPlayerABizz(playerid)][aSlet] == 0) Send(playerid, 0xFFE569FF, "Anda memiliki beberapa hari gaji tersisa untuk pompa bensin."); }
	if(PlayerInfo[playerid][pText] == 1) PlayerInfo[playerid][pText] = 0, Send(playerid, COLOR_LIGHTRED, "Anda menerima peringatan offline, lebih detail /warninfo");
	if(PlayerInfo[playerid][pText] == 3 && PlayerInfo[playerid][pfWarn] > 0) PlayerInfo[playerid][pText] = 0, SCMF(playerid, COLOR_LIGHTRED, "Anda menerima peringatan sebagai pemimpin faksi %d/3", PlayerInfo[playerid][pfWarn]);
	if(PlayerInfo[playerid][pText] == 4) PlayerInfo[playerid][pText] = 0, Send(playerid, COLOR_LIGHTRED, "Anda dikeluarkan dari kepemimpinan fraksi offline.");

	if(PlayerInfo[playerid][pMestoPrison] >= 1 || PlayerInfo[playerid][pPrison] >= 1)
	{
	    if(PlayerInfo[playerid][pMestoPrison] == 1) // LSPD
	    {
			t_SetPlayerPos(playerid, 686.6003,2552.3213,-94.6045);
			SetPlayerInterior(playerid, 6), SetPlayerVirtualWorld(playerid, 1);
		}
		else if(PlayerInfo[playerid][pMestoPrison] == 2) // FBI
		{
			t_SetPlayerPos(playerid,198.4545,161.8394,1003.0300);
			SetPlayerInterior(playerid, 3), SetPlayerVirtualWorld(playerid, 2);
		}
		else if(PlayerInfo[playerid][pMestoPrison] == 3) // PRISON
		{
		    t_SetPlayerPos(playerid, 3518.8623,1075.6036,17.1577);
			SetPlayerInterior(playerid, 0), SetPlayerVirtualWorld(playerid, 1);
		}
		SetCameraBehindPlayer(playerid), ResetWeapons(playerid);
	}

    if(PlayerInfo[playerid][pVipTime] > 0)
	{
		new current_date = getdate(year, month, day);
		new time_left = PlayerInfo[playerid][pVipTime] - current_date;
		if(time_left <= 0)
		{
			SendClientMessage(playerid, COLOR_YELLOW, "Status Anda kedaluwarsa!");
			PlayerInfo[playerid][pVip] = PlayerInfo[playerid][pVipTime] = 0;
			OnPlayerUpdateAccountsPer(playerid, "pVip", PlayerInfo[playerid][pVip]);
			OnPlayerUpdateAccountsPer(playerid, "pVipTime", PlayerInfo[playerid][pVipTime]);
		}
	}

	SetTimerEx("UpdatePlayerDataAfterLogin", 3000, false, "i", playerid);

	SpawnPlayerEx(playerid);
	return 1;
}

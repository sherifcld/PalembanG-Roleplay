    new Cache: result = mysql_query(mMysql, "SELECT s.*, a.`pName`, a.`pOnline` FROM `supports` s, `accounts` a WHERE a.`pID` = s.`user_id`");

    new rows = cache_num_rows();

    if(!rows) return SCM(playerid, COLOR_GREY, "Dukungan tidak ditemukan.");

    new player_name[MAX_PLAYER_NAME], last_date[16], sql_id, reports_today;

    new count = 0;

    String = "";

    new str[48];

    for(new i; i < rows; i++)
    {
        cache_get_value_int(i, "user_id", sql_id);
        cache_get_value_name(i, "pName", player_name, MAX_PLAYER_NAME);
        cache_get_value_name(i, "pOnline", last_date, 16);
        cache_get_value_int(i, "reports_day", reports_today);

        format(str, 48, "%i. %s\t%i\t%s\n", i + 1, player_name, reports_today, last_date);
        strcat(String, str);

        SetPlayerListitemValue(playerid, count++, sql_id);
    }

    cache_delete(result);

    return SPD(playerid, 9137, DIALOG_STYLE_TABLIST, "Daftar Dukungan", String, "Pilih", "Tutup");
}


public OnPlayerEnterCheckpoint(playerid)
{
	new vehicleid = GetPlayerVehicleID(playerid);

	if(PlayerInfo[playerid][pJob] == 2 && pTemp[playerid][pBusID] != 0 && GetPlayerVehicleID(playerid) == pTemp[playerid][pBusID])
    {
        new	pos = busPos{playerid}, route = player_route_id[playerid];

        if(gBusCPs[route][pos][0] == gBusCPs[route][pos+1][0] && gBusCPs[route][pos][1] == gBusCPs[route][pos+1][1])
        {
            format(format_string,24+100,"Bus tiba di stasiun, berangkat dalam 10 detik!");//,r_name);
            ProxDetector(30.0,playerid,format_string,0x44b2ffff,0x44b2ffff,0x44b2ffff,0x44b2ffff,0x44b2ffff);
            SetTimerEx("bus_stop",10000,false,"ii",playerid,route);
            DisablePlayerCheckpoint(playerid);
            return 1;
        }
        if(pos == 99 || gBusCPs[route][pos+1][0] == 0.0) busPos{playerid} = 0;
		else busPos{playerid}++;
		SetNextCP(playerid,busPos{playerid});
    }

    if(vehicleid != INVALID_VEHICLE_ID && player_product_id[playerid] != -1)
    {
        new order_id = player_product_id[playerid];

		new amount = vehicle_load_prod[vehicleid];

        new reward = amount;

        new bool: is_product = GetVehicleModel(vehicleid) == 414 ? true : false;

        if(is_product && PlayerToPoint(15.0, playerid, BizzInfo[order_id][bEntranceX], BizzInfo[order_id][bEntranceY], BizzInfo[order_id][bEntranceZ]))
        {
            if(amount == 0) return SCM(playerid, COLOR_GREY, "Anda belum mengunduh produk(/load).");

            BizzInfo[order_id][bProducts] += amount;

            SaveBizzPer(order_id, "bProducts", BizzInfo[order_id][bProducts]);

            SCMF(playerid, COLOR_GREEN, "Anda dibongkar %i pendapatan produk: $%i", amount, reward);

			GivePlayerMoneyEx(playerid, reward, "pendapatan untuk produk");

            vehicle_load_prod[vehicleid] = 0;
            player_product_id[playerid] = -1;
		}

		if(!is_product && PlayerToPoint(15.0, playerid, BizzAInfo[order_id][aAreaFillX], BizzAInfo[order_id][aAreaFillY], BizzAInfo[order_id][aAreaFillZ]))
        {
            if(amount == 0) return SCM(playerid, COLOR_GREY, "Anda belum memuat bahan bakar(/load).");

            BizzAInfo[order_id][aProducts] += amount;

            SaveABizzPer(order_id, "aProducts", BizzAInfo[order_id][aProducts]);

            SCMF(playerid, COLOR_GREEN, "Anda dibongkar %i pendapatan bahan bakar: $%i", amount, reward);

            GivePlayerMoneyEx(playerid, reward, "pendapatan bahan bakar");

            vehicle_load_prod[vehicleid] = 0;
            player_product_id[playerid] = -1;
		}
    }

    if(IsPlayerInAnyVehicle(playerid) && rent_job_vehicle[playerid] == GetPlayerVehicleID(playerid) && pTemp[playerid][pTrucker] > -1 && PlayerInfo[playerid][pJob] == 6)
	{
		switch(pTemp[playerid][pTrucker])
		{
			case 0:
			{
				TogglePlayerControllable(playerid, false);
				SPD(playerid, 232, DIALOG_STYLE_INPUT, CAPTION_NUMBER_OF_CARGO, INFO_NUMBER_OF_CARGO, ">", "X");
			}
		 	case 1:
		 	{
				TogglePlayerControllable(playerid, false);
				SendClientMessage(playerid, -1, !"Tunggu bongkar muat");
				SetTimerEx("@TruckerTimer", pTemp[playerid][pTrucker_Unit]*2, false, "i", playerid);
			}
		}
	}

	if(PlayerInfo[playerid][pJob] == WORK_TYPE_BULLDOZER && VehInfo[vehicleid][vFr] == WORK_TYPE_BULLDOZER)
	{
		if(bd_player_state[playerid] == 1)
		{
		    TogglePlayerControllable(playerid, false);

		    SetTimerEx("LoadPlayerBulldozer", 3000, false, "i", playerid);

		    SCM(playerid, -1, "Tunggu sampai buldoser selesai memuat.");
		}
		else if(bd_player_state[playerid] == 2)
		{
		    DestroyDynamicObject(bd_veh_obj[vehicleid]);
		    bd_veh_obj[vehicleid] = 0;

		    bd_player_state[playerid] = 1;

		    new reward = 200 + random(200);

		    SCMF(playerid, -1, "Ditambahkan ke gaji: $%i, akan dikreditkan ke akun PayDay Anda.", reward);

		    GivePlayerPayCheck(playerid, reward);

		    UpdatePlayerBulldozerMark(playerid);
		}

		return 1;
	}
	if(vehicleid == rent_job_vehicle[playerid] && PlayerInfo[playerid][pJob] == WORK_TYPE_DELIVERY)
	{
	    if(delivery_state_id[playerid] == 1)
	    {
	        if(crane_unload_use != INVALID_PLAYER_ID)
	            return SCM(playerid, COLOR_GREY, "Harap tunggu saat memuat sedang sibuk.");

	        if(crane_unload_refresh != 0)
	            return SCM(playerid, COLOR_GREY, "Tunggu sampai wadahnya siap.");

	    	new crane_id = MAX_CRANES;

	        crane_unload_use = playerid;
	        crane_unload_refresh = 30;

	        SetVehiclePos(vehicleid, crane_unload_veh[0], crane_unload_veh[1], crane_unload_veh[2]);
	        SetVehicleZAngle(vehicleid, crane_unload_veh[3]);

	        TogglePlayerControllable(playerid, false);

	        new time = 10000;

	        for(new i; i < 4; i++)
			{
		    	time = MoveDynamicObject(
					crane_unload_obj[i],
				  	crane_stop_pos[crane_id][i][0],
		 			crane_stop_pos[crane_id][i][1],
					crane_stop_pos[crane_id][i][2]-0.004,
					0.4,
					crane_stop_pos[crane_id][i][3],
					crane_stop_pos[crane_id][i][4],
					crane_stop_pos[crane_id][i][5]
				);
			}

			SCM(playerid, -1, "Pengunduhan dimulai, tunggu..");

	        SetTimerEx("LoadPlayerContainer", time, false, "i", playerid);
		}
		else if(2 <= delivery_state_id[playerid] <= 3)
		{
		    new crane_id = delivery_state_id[playerid] - 2;

		    if(crane_use[crane_id] != INVALID_PLAYER_ID)
		        return SCM(playerid, COLOR_GREY, "Situs ini sedang sibuk.");

			if(crane_refresh[crane_id] != 0)
			    return SCM(playerid, COLOR_GREY, "Tunggu sampai debit selesai..");

            crane_use[crane_id] = playerid;

		    SetVehiclePos(vehicleid, crane_veh_pos[crane_id][0], crane_veh_pos[crane_id][1], crane_veh_pos[crane_id][2]);
		    SetVehicleZAngle(vehicleid, crane_veh_pos[crane_id][3]);

		    TogglePlayerControllable(playerid, false);

		    DestroyDeliveryObject(playerid);

		    crane_obj[crane_id][2] = CreateDynamicObject
			(
				2935,
				crane_stop_pos[crane_id][2][0],
				crane_stop_pos[crane_id][2][1],
				crane_stop_pos[crane_id][2][2],
				crane_stop_pos[crane_id][2][3],
				crane_stop_pos[crane_id][2][4],
				crane_stop_pos[crane_id][2][5]
			);

		    new time = 10000;

	        for(new i = 1; i != -1; i--)
			{
		    	time = MoveDynamicObject(
					crane_obj[crane_id][i],
				  	crane_stop_pos[crane_id][i][0],
		 			crane_stop_pos[crane_id][i][1],
					crane_stop_pos[crane_id][i][2]-0.004,
					0.4,
					crane_stop_pos[crane_id][i][3],
					crane_stop_pos[crane_id][i][4],
					crane_stop_pos[crane_id][i][5]
				);
			}

		    SCM(playerid, -1, "Bongkar kontainer dimulai, tunggu.");

		    SetTimerEx("UnloadPlayerContainer", time, false, "i", playerid);
		}

		return 1;
	}
	if(GetPVarInt(playerid, "GPS") != 0)
	{
		SetPVarInt(playerid, "GPS", 0);
	}
	if(GetPVarInt(playerid,"CHECKPOINTID") == 3 && !IsPlayerInAnyVehicle(playerid) && player_job[playerid] == 2)
	{
		SetPlayerAttachedObject(playerid, 1, 1558, 1, 0.206000,0.440999,-0.036000,0.000000,270.000000,0.000000,-0.449999,-0.449999,-0.449999);
		SendClientMessage(playerid,-1,"Anda mengambil kotak itu, bawa ke trailer.");
		ApplyAnimation(playerid,"CARRY","putdwn",4.0,0,1,1,0,0,1);
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_CARRY);
		DisablePlayerCheckpoint(playerid);
		new rand = random(4);
		SetPlayerRaceCheckpoint(playerid,1,work_pos_gruzchiki[1][rand][0],work_pos_gruzchiki[1][rand][1],work_pos_gruzchiki[1][rand][2],0.0,0.0,0.0,3);
		SetPVarInt(playerid,"CHECKPOINTID",4);
	}
	if(GetPVarInt(playerid,"CHECKPOINTID") == 5 && !IsPlayerInAnyVehicle(playerid) && IsPlayerInRangeOfPoint(playerid, 5.0, 2564.6721,-1292.9766,1044.1250))
	{
		new bablo[33];
		SetPlayerSpecialAction (playerid, SPECIAL_ACTION_NONE);
		if(IsPlayerAttachedObjectSlotUsed(playerid,2)) RemovePlayerAttachedObject(playerid,2);
		GivePlayerMoneyEx(playerid, 250, "pemuat"); //TEST
		SetPVarInt(playerid,"JobMoney",GetPVarInt(playerid,"JobMoney")+250);
		format(bablo,32,"~g~+250$~n~~b~TOTAL:~w~ %i$",GetPVarInt(playerid,"JobMoney"));
		GameTextForPlayer(playerid,bablo, 5000, 1);
		ClearAnimations(playerid);
		DeletePVar(playerid,"CHECKPOINTID");
		DisablePlayerCheckpoint(playerid);
	}
	if(GetPVarInt(playerid,"Saw_"))
	{
		if(player_job[playerid] == 1 && !IsPlayerInAnyVehicle(playerid) && GetPVarInt(playerid,"Saw") != 1)
		{
			ApplyAnimation(playerid,"CHAINSAW","WEAPON_csaw",4.0,1,1,1,1,0);
			SetPVarInt(playerid,"AntiSbiv",gettime()+10);
			SetPVarInt(playerid,"Saw",1);
			DisablePlayerCheckpoint(playerid);
			SendClientMessage(playerid,0x27C400FF,"Anda mulai menggergaji pohon");
		}
	}
	//if(IsPlayerInVehicle(playerid, specialcar))
    if(PlayerInfo[playerid][pJob] == 3 && pTemp[playerid][puborkacar] != 0)
    {
        new pos = uborkaPos{playerid},route = player_route_id[playerid];
        if(pos == 99 || gUborkaCPs[route][pos+1][0] == 0.0) uborkaPos{playerid} = 0;
		else uborkaPos{playerid}++;
		SetNextCP(playerid,uborkaPos{playerid});
    }
    if(GetPVarInt(playerid, "Sad") == 1 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
    {
        TogglePlayerControllable(playerid, false);

        SetTimerEx("SadYablkJob",5000,0,"%i",playerid);
        GameTextForPlayer(playerid,"~y~+1",5200,5);
        AmmountWood[playerid] += 1;

        //format(string, 65,"%d",AmmountWood[playerid]);
        //PlayerTextDrawSetString(SADD[playerid],string);
        //format(string, 65,"%d",AmmountWood[playerid]*2);
        //PlayerTextDrawSetString(SADD1[playerid],string);

        ApplyAnimation(playerid,"BOMBER","BOM_Plant",4.0,1,1,1,1,0);
        new rand = random(sizeof(derevo));
        SetPlayerCheckpoint
		(
			playerid,
			derevo[rand][0],
			derevo[rand][1],
			derevo[rand][2],
			3.0
	    );
        DeletePVar(playerid, "Sad");
        DisablePlayerCheckpoint(playerid);
        SetPVarInt(playerid, "Sad", 1);

        if(beg_quest_info[playerid][b_quest_id] == QUEST_TYPE_APPLE && beg_quest_info[playerid][b_quest_status] == 1)
		{
		    beg_quest_info[playerid][b_quest_progress]++;
		    OnPlayerUpdateAccountsPer(playerid, "b_quest_progress", beg_quest_info[playerid][b_quest_progress]);

            SCMF(playerid, COLOR_YELLOW, "[QUEST] {FFFFFF}Masih mengumpulkan apel: %i/40", beg_quest_info[playerid][b_quest_progress]);

		    if(beg_quest_info[playerid][b_quest_progress] >= 40) //40 apel
		    {
		        SCM(playerid, COLOR_YELLOW, "[QUEST] {FFFFFF}Anda telah mengumpulkan jumlah apel yang tepat, menyelesaikan pekerjaan dan mengembalikannya.");

		        beg_quest_info[playerid][b_quest_status] = 2;

		        OnPlayerUpdateAccountsPer(playerid, "b_quest_status", 2);
		    }
		}

        return true;
    }
    DisablePlayerCheckpoint(playerid);
	return 1;
}


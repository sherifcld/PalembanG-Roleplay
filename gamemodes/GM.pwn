		if(PlayerToPoint(2.0,playerid,BizzInfo[h][bBarX],BizzInfo[h][bBarY],BizzInfo[h][bBarZ]) && GetPlayerVirtualWorld(playerid) == BizzInfo[h][bVirtualWorld])
		{
		    player_use_bizbar[playerid] = h;
			switch(BizzInfo[h][bType])
			{
			    case 1: PC_EmulateCommand(playerid, "/drink");
			    case 2: PC_EmulateCommand(playerid, "/eat");
			    case 3,7: PC_EmulateCommand(playerid, "/buy");
			    case 5: PC_EmulateCommand(playerid, "/lease");
			    case 6: PC_EmulateCommand(playerid, "/clothes");
			    case 8: ShowPlayerBizzAmmoMenu(playerid);
			    case 23:
			    {
			        if(PlayerInfo[playerid][pLevel] < 2) return SCM(playerid, COLOR_GREY, "Tersedia dari level 2.");

			        if(pb_state != 1) return SCM(playerid, COLOR_GREY, "Pendaftaran saat ini ditutup.");

			        if(player_reg_pb[playerid] != -1) return SCM(playerid, COLOR_GREY, "Anda sudah terdaftar.");

                    if(PlayerInfo[playerid][pCash] < 2000) return SCM(playerid, COLOR_GREY, "diperlukan $2.000 untuk pendaftaran");

                    PlayerInfo[playerid][pCash] -= 2000;

                    GiveBusinessMoney(h, 500, 1);

                    pb_reward += 2000;

			        player_reg_pb[playerid] = pb_next_team;

			        pb_next_team = !pb_next_team;

			        SCMF(playerid, -1, "Anda terdaftar di Paint Ball, tim Anda: %s", player_reg_pb[playerid] ? ("Merah") : ("Biru"));
			    }
   			}
			return 1;
		}
	}
	for(new i = 1; i <= TotalEnters; i++)
	{
		if(pickupid == EntersInfo[i][pickPickup])
		{
		    switch(EntersInfo[i][pickID])
		    {
		        case 5: // Pembayaran Properti
				{
					new text[32], text1[36], text2[32];
					if(GetPlayerHouse(playerid) == -1) text = "{FF6C00}Tidak di rumah";
					else format(text, sizeof(text), "{36DE00}Nomor rumah%d", GetPlayerHouse(playerid));

					if(GetPlayerBizz(playerid) == -1) text1 = "{FF6C00}Tidak ada bisnis";
					else format(text1, sizeof(text1), "{36DE00}%s (No%d)", BizzInfo[GetPlayerBizz(playerid)][bMessage], GetPlayerBizz(playerid));

					if(GetPlayerABizz(playerid) == 0) text2 = "{FF6C00}Tidak ada pom bensin";
					else format(text2, sizeof(text2), "{36DE00}%s (No%d)", BizzAInfo[GetPlayerABizz(playerid)][aMessage], GetPlayerABizz(playerid));

				    format(String, 244, "{ffffff}1. Pembayaran rumah\t%s\n{ffffff}2. Pembayaran bisnis\t%s\n{ffffff}3. Pembayaran untuk pom bensin\t%s", text, text1, text2);
					SPD(playerid,107, DIALOG_STYLE_LIST,"Pembayaran Properti", String, "Pilih", "Keluar");
					ProxDetectorBot(20.0, player_enter_actors[playerid], "Halo! Apa yang ingin kamu bayar?",COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE5,COLOR_FADE5);
				}
				case 6:
				{
				    SetPlayerUseListitem(playerid, 0);

					MenuAtms(playerid);//BankDialog(playerid);
				}
				case 8: // karena rasa sakit
				{
					if(CheckHospitalHealth(playerid)) return 1;

					t_SetPlayerPos(playerid,EntersInfo[i][pickPosX], EntersInfo[i][pickPosY],EntersInfo[i][pickPosZ]), SetPlayerFacingAngle(playerid, EntersInfo[i][pickPosA]);
					SetPlayerInterior(playerid, EntersInfo[i][pickPosI]), SetPlayerVirtualWorld(playerid, EntersInfo[i][pickPosV]);
					SetCameraBehindPlayer(playerid);
					use_bd(playerid);
				}
				case 9:
				{
				    if(PlayerInfo[playerid][pJob] == 1) return Send(playerid, COLOR_GREY, "Anda sudah bekerja sebagai distributor makanan/bahan bakar. Gunakan /leave untuk berhenti.");

					format(format_string, sizeof(format_string), "Penempatan kerja: {09E097}Pengangkut Makanan/Bahan Bakar\n"W"Anda sekarang: %s\n\nAnda yakin ingin mendapatkan pekerjaan ini?", NamesJob(playerid));
					SPD(playerid, 152, DIALOG_STYLE_MSGBOX,"Penempatan kerja", format_string, "Ya", "Tidak");
				}
				case 20: return 1; //gudang tentara terpencil
 				case 23: // mats LSPD
				{
				    if(GetPVarInt(playerid, "take_mats"))
				    {
				        new m_id = FRACTION_LSPD;

				        if(FracInfo[m_id][fBank][0] + 250 > 100000 || FracInfo[m_id][fBank][1] + 250 > 100000)
							return SCM(playerid, COLOR_GREY, "Sudah ada banyak persediaan amunisi.");

				        SCM(playerid, COLOR_GREEN, "Anda menaruh amunisi di gudang LSPD.");
						SCMF(playerid, -1, "Status Stok: %i/100000", FracInfo[m_id][fBank][0]);

				        FracInfo[m_id][fBank][0] += 250;
				        FracInfo[m_id][fBank][1] += 250;

				        return RemovePlayerAmmoBox(playerid);
				    }

				    if(PlayerInfo[playerid][pMember] != 1) return Send(playerid, COLOR_GREY, "Hanya karyawan LSPD yang dapat menggunakan gudang..");

				    return ShowPlayerAmmoDialog(playerid);
	   			}
				/*case 18:
				{
				    format(String, sizeof(String), "1. Penciptaan keluarga\n2. Informasi\n3. Total keluarga di negara bagian: {fbec5d}%d", TotalFamily);
					SPD(playerid, 204, DIALOG_STYLE_LIST, "{E5D81E}Informasi Keluarga", String, "Pilihan", "Keluar");
					return 1;
				}*/
				case 25:
				{
				    if(player_news_live[playerid]) return SCM(playerid, COLOR_GREY, "Anda tidak dapat meninggalkan gedung saat di udara (/leave).");

				    SetPPos(playerid, EntersInfo[i][pickPosX], EntersInfo[i][pickPosY],EntersInfo[i][pickPosZ], EntersInfo[i][pickPosA], EntersInfo[i][pickPosI], EntersInfo[i][pickPosV], true);
				}
				default:
				{
				    SetPPos(playerid, EntersInfo[i][pickPosX], EntersInfo[i][pickPosY],EntersInfo[i][pickPosZ], EntersInfo[i][pickPosA], EntersInfo[i][pickPosI], EntersInfo[i][pickPosV], true);

					use_bd(playerid);
				}
      		}
			return true;
		}
	}
	return 1;
}


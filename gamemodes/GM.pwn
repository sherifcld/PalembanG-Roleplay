epublic: OnPlayerBanIP(playerid, ipadress[], reason[],  bool:iplocktype, days)
{
    new rows;
    cache_get_row_count(rows);
    if(rows) return Send(playerid,-1, "Alamat IP ini sudah diblokir saat ini!");
    switch(iplocktype)
	{
	    case 0:
	    {
		   	format(format_string, sizeof(format_string), "[A] Admin %s diblokir Alamat IP: [%s]. Untuk istilah tersebut: %d hari. Alasan: %s", Name(playerid), ipadress, days, reason);
			SendAdminMessage(COLOR_GREY, format_string);
			AddAdminLog(playerid, format_string);

			BlockIpAdress(ipadress, reason, Name(playerid), gettime() + (86400 * days));
	    }
	    default:
	    {
	        format(format_string, sizeof(format_string), "[A] Variabel %s Alamat IP yang Diblokir: [%s]. dengan alasan: %s", Name(playerid), ipadress, reason);
			SendAdminMessage(COLOR_GREY, format_string);
			AddAdminLog(playerid, format_string);

			BlockIpAdress(ipadress, reason, Name(playerid), 2147483647);
	    }
   	}
	return true;
}
epublic: BlockIpAdress(ipadress[], reason[], admin[], unbandate)
{
	mysql_format(mMysql, MySQLStr, 344, "INSERT INTO `banips` (`ipadress`, `reason`, `admin`, `date`, `unbandate`, `lockstate`) VALUES ('%e', '%e', '%e', '%i', '%i', '1')", ipadress, reason, admin, gettime(), unbandate);
	mysql_tquery(mMysql, MySQLStr, "", "");

	foreach(new i : Player)
	{
		if(!strcmp(GetPlayerIpEx(i), ipadress)) KickD(i, "Alamat IP Anda diblokir, silakan kembali untuk mendapatkan informasi.");
	}
	return true;
}
epublic: BlockAccounts(name[], reason[], admin[], unbandate)
{
	mysql_format(mMysql, MySQLStr, 412, "INSERT INTO `banlog` (`nameplayer`, `reason`, `nameadmin`, `date`, `unbandate`, `lockstate`) VALUES ('%e', '%e', '%e', '%i', '%i', '1')", name, reason, admin, gettime(), unbandate);
	mysql_tquery(mMysql, MySQLStr, "", "");
	//
	foreach(new i : Player) if(!strcmp(name, Name(i))) KickD(i, "");
	return true;
}

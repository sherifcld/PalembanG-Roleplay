CMD:tie(playerid, params[])
{
	if(!IsAMafia(playerid)) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");

	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SCM(playerid, COLOR_GREY, "Anda harus berada di kursi pengemudi.");

	new to_player;

    if(sscanf(params, "u", to_player)) return Send(playerid, -1,"Masuk: /tie [id pemain]");

    if(GetPlayerVehicleID(playerid) != GetPlayerVehicleID(to_player)) return Send(playerid, COLOR_GREY,"Anda tidak berada dalam transportasi yang sama dengan pemain ini.");

    if(!player_tie[to_player])
    {
        SCMF(playerid, -1, "Anda terikat %s", GetPlayerNameEx(to_player));
        SCMF(to_player, -1, "%s menghubungkan Anda", GetPlayerNameEx(playerid));

        format(format_string, 188, "%s diikat %s",Name(playerid), GetPlayerNameEx(to_player));
    }
    else
    {
        SCMF(playerid, -1, "Anda melepaskan %s", GetPlayerNameEx(to_player));
        SCMF(to_player, -1, "%s melepaskan Anda", GetPlayerNameEx(playerid));

        format(format_string, 188, "%s dilepaskan %s",Name(playerid), GetPlayerNameEx(to_player));
    }

	TogglePlayerControllable(to_player, player_tie[to_player]);

	ProxDetector(20.0, playerid, format_string, 0xDD90FFFF,0xDD90FFAA,0xDD90FFAA,0xDD90FFAA,0xDD90FFAA);

	SetPlayerChatBubble(playerid, !player_tie[to_player] ? ("dilepaskan") : ("diikat"),0xDD90FFFF,20.0,10000);

    player_tie[to_player] = !player_tie[to_player];

	return 1;
}


stock ShowStats(playerid, params)
{
    if(PlayerInfo[params][pLogin] == 0) return 1;
    new str[324];
    
    new atext[20];
    if(PlayerInfo[params][pSex] == 1) atext = "Male";
    else atext = "Famale";

    new Float: hp;
    GetPlayerHealth(params, hp);
    new Float: am;
    GetPlayerArmour(params, am);

    new htext[32], h_id = GetPlayerHouse(params);
    if(h_id != -1) format(htext, sizeof(htext), "House%d ($%i)", h_id, HouseInfo[h_id][hValue]);
    else if(PlayerInfo[playerid][pHotel] != -1) format(htext, sizeof(htext), "Hotel %d", PlayerInfo[playerid][pHotel]);
    else htext = "Gelandangan";

    new btext[64], b_id = GetPlayerBizz(params);
    if(b_id == -1) btext = "None";
    else format(btext, sizeof(btext), "%s ($%i)", BizzInfo[b_id][bMessage], BizzInfo[b_id][bMoney]);

    new azctext[64], a_id = GetPlayerABizz(params);
    if(a_id == 0) azctext = "None";
    else format(azctext, sizeof(azctext), "%s ($%i)", BizzAInfo[a_id][aMessage], BizzAInfo[a_id][aMoney]);

    new drank[20];
    switch(PlayerInfo[params][pLevel])
    {
        case 1..2: drank = "Player Baru |";
        case 3..5: drank = "Player Baru ||";
        case 6..11: drank = "Player Senior |";
        case 12..15: drank = "Player Senior ||";
        case 16..20: drank = "Player Old";
        case 21..25: drank = "Player Master";
        default: drank = "Sepuh Server";
    }

    new infofrac[32];
    if(PlayerInfo[params][pMember] == 0) infofrac = "-";
    else format(infofrac, sizeof(infofrac), "%s", FracInfo[PlayerInfo[params][pMember]][fName]);

    new infojob[64];
    if(PlayerInfo[params][pJob] == 0 && PlayerInfo[params][pMember] == 0) infojob = "None";
    else if(PlayerInfo[params][pJob] != 0) format(infojob, sizeof(infojob), "%s", NamesJob(params));
    else if(PlayerInfo[params][pMember] != 0) format(infojob, sizeof(infojob), "%s", FracRank[PlayerInfo[params][pMember]][PlayerInfo[params][pRank]]);

    new idiot[212];
    format(idiot, sizeof(idiot), ""WHITE_E"%s (%d)", Name(params), PlayerInfo[playerid][pID]);
    String = "";
    format(str, sizeof(str), "\
        {FFFF00}In Character:{FFFFFF}\n\
        {ffffff}Gender: [%s] Phone number: [%d] Phone credit: [%d]\n\
        Jobs: [%s] Money :["GREEN_E"$%d"WHITE_E"] Bank ["GREEN_E"$%d"WHITE_E"]\n",
        atext, PlayerInfo[params][pNumber], PlayerInfo[params][pSummaNumber], infojob,
        PlayerInfo[params][pCash],PlayerInfo[params][pBank]);
    strcat(String,str);
    
    format(str, sizeof(str), "\
    Faction: [%s] Position: [%s]\n\
    House: [%s] Business: [%s] Fuel Pomp:[%s]\n", infofrac, infojob, htext, btext, azctext);

    strcat(String,str);

    {
        strcat(String, "Vehicles:\n");

        for(new i; i < MAX_PLAYER_VEHICLES; i++)
        {
            new model = CarInfo[params][cModel][i];
            new price = CarInfo[params][cCost][i];

            if(model == 0)
            {
                format(str, 48, "%i. None\n", i + 1);
                strcat(String, str);
            }
            else
            {
                format(str, 48, "%i. %s (Rp%i)\n", i + 1, VehicleNames[model - 400], price);
                strcat(String, str);
            }
        }

        format(str, 500,
            "\n{FFFF00}Out Of Character:{FFFFFF}\n\
            Warn: ["RED_E"%d/3"WHITE_E"] VIP: ["YELLOW_E"%s{ffffff}"WHITE_E"] GOLD: ["YELLOW_E"%d"WHITE_E"]\n\
            Level: [%d] [%s] Health: ["RED_E"%.0f"WHITE_E"] Armour: ["RED_E"%.0f"WHITE_E"]\n\
            Device: [%s]",
            PlayerInfo[params][pWarn], VipNames(params), PlayerInfo[params][pRouble],
            PlayerInfo[params][pLevel], drank, hp, am,
            IsPlayerMobile(params) ? ("Android") : ("PC"));

        strcat(String ,str);
    }

    SPD(playerid,0,DIALOG_STYLE_TABLIST_HEADERS, idiot, String, "Close", "");
    return 1;
}

CMD:ad(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pMuted] > 0) return PlayerMuted(playerid);
    if(PlayerInfo[playerid][pLevel] < 2) return Send(playerid, COLOR_GREY, "Iklan dapat dikirimkan memiliki 2 level permainan!");
    if(PlayerInfo[playerid][pProducts][0] == 0) return Send(playerid, COLOR_GREY, "Anda tidak memiliki ponsel.");
    if(advert_block[playerid] > gettime()) return SCM(playerid,COLOR_GREY,"Tidak satu menit berlalu dari iklan yang terakhir dikirim");
    //if(lNews[playerid] == 1) return Send(playerid, -1, "Nyalakan radio terlebih dahulu");
    if(sscanf(params, "s[60]", params[0])) return Send(playerid, -1, "Masuk: /ad [teks]");
    if(strfind(params[0], "{", true) != -1) return Send(playerid,-1,"Anda memasukkan tanda larangan! ( { )");
    if(PlayerInfo[playerid][pSummaNumber] < 200) return Send(playerid, -1, "Tidak cukup uang di saldo ponsel Anda!");

    for(new i; i<MAX_ADVERTS; i++)
    {
		if(AdvertInfo[i][aName]!=0) continue;

		format(AdvertInfo[i][aName],24,GN(playerid));
		format(AdvertInfo[i][aText],128,params[0]);
        AdvertInfo[i][aNumber]=PlayerInfo[playerid][pNumber];
	    PlayerInfo[playerid][pSummaNumber] -= 200;
	    format(format_string,80,"[SA News] Para editor menerima pengumuman dari {FFFFFF}%s[%d]",GN(playerid),playerid);
	    SendFrac(6,COLOR_GREEN,format_string);

	    advert_block[playerid] = gettime() + 60;

	    SCM(playerid, COLOR_WHITE, "Iklan Anda telah dikirim untuk ditinjau, telepon Anda telah dibebankan biaya: $200");
		return SCM(playerid, COLOR_GREEN, "Tunggu, sementara staf editorial sedang mengeditnya!");
	}
	return SCM(playerid,COLOR_GREY,"Studio memiliki banyak iklan, tunggu, sementara mereka dilihat");
}


CMD:present(playerid, params[])
{
	if(GetPlayerWeapon(playerid) != 14) return Send(playerid,-1,"Ambil bunga untuk diberikan.");
	if(sscanf(params, "d", params[0])) return Send(playerid, -1, "Masuk: /present [ID pemain]");

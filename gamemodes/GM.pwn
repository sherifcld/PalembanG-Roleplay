            {
                new Float: pos[3];

                cache_get_value_name(i, "hUpdStorePos", result_string);
                sscanf(result_string, "p<,>a<f>[3]", pos);

                CreateHouseStore(i, pos[0], pos[1], pos[2]);

                cache_get_value_name(i, "hWeaponID", result_string);
                sscanf(result_string, "p<,>a<i>[3]", HouseInfo[i][hWeaponID]);

                cache_get_value_name(i, "hAmmo", result_string);
                sscanf(result_string, "p<,>a<i>[3]", HouseInfo[i][hAmmo]);

                cache_get_value_name_int(i, "hDrugs", HouseInfo[i][hDrugs]);
            }

            cache_get_value_name_float(i, "hHealpickX", HouseInfo[i][hHealpickX]);
            cache_get_value_name_float(i, "hHealpickY", HouseInfo[i][hHealpickY]);
            cache_get_value_name_float(i, "hHealpickZ", HouseInfo[i][hHealpickZ]);
           	cache_get_value_name_int(i, "hSlet", HouseInfo[i][hSlet]);

           	cache_get_value_name(i, "hLodgers", result_string);

           	SplitString(result_string, house_lodgers[i], ',');

           	if(HouseInfo[i][hSlet] != 0)
           	{
				format(format_string, 144, "UPDATE `accounts` SET `pBank` = `pBank` + %i WHERE `pName` = '%s'", HouseInfo[i][hValue] / 2, HouseInfo[i][hOwner]);
				mysql_tquery(mMysql, format_string);

				strmid(HouseInfo[i][hOwner], "None", 0, strlen("None"), 64);
			    HouseInfo[i][hOwned] = 0;
				HouseInfo[i][hHel] = 0;
				HouseInfo[i][hLock] = 0;
				HouseInfo[i][hUpdAD] = 0;
				HouseInfo[i][hUpdHel] = 0;
				HouseInfo[i][hUpdSub] = 0;
				HouseInfo[i][hUpdStore] = 0;
				HouseInfo[i][hSlet] = 0;
				HouseInfo[i][hOplata] = 0;

				DestroyHouseStore(i);

				SaveHouse(i);
           	}

           	if(HouseInfo[i][hOwned] == 0)
			{
				if(HouseInfo[i][hKlass] == 5) HouseInfo[i][hPickup] = CreatePickup(1273, 23, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],-1);
				else HouseInfo[i][hPickup] = CreatePickup(1273, 23, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],-1);
				HouseInfo[i][hMIcon] = CreateDynamicMapIcon(HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez], 31, -1, 0, -1, -1);
			}
            if(HouseInfo[i][hOwned] == 1)
			{
	    		if(HouseInfo[i][hSlet] == 2) HouseInfo[i][hPickup] = CreatePickup(1272, 23, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],-1);
				else if(HouseInfo[i][hKlass] == 5) HouseInfo[i][hPickup] = CreatePickup(1272, 23, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],-1);
				else HouseInfo[i][hPickup] = CreatePickup(1272, 23, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],-1);
				HouseInfo[i][hMIcon] = CreateDynamicMapIcon(HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez], 32, -1, 0, -1, -1);
			}
			if(HouseInfo[i][hUpdHel] == 1)
			{
		 		HouseInfo[i][hAreaHeal] = CreateDynamicPickup(11738, 23, HouseInfo[i][hHealpickX], HouseInfo[i][hHealpickY], HouseInfo[i][hHealpickZ], i);
		 		format(String, 64, "{E5D81E}Kotak P3K: %d pcs.", HouseInfo[i][hHel]);
 				healtext[i] = CreateDynamic3DTextLabel(String,0xFFFFFFFF,HouseInfo[i][hHealpickX],HouseInfo[i][hHealpickY],HouseInfo[i][hHealpickZ]+0.5, 10.0,INVALID_PLAYER_ID,INVALID_VEHICLE_ID,1, i);
 			}
 			new h_class = HouseInfo[i][hKlass];
			HouseInfo[i][hAreaExit] = CreateDynamicSphere(house_exit[h_class][0], house_exit[h_class][1], house_exit[h_class][2],3,i);
			HouseInfo[i][hAreaEnter] = CreateDynamicSphere(HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez],1.2,0);
		}
		printf("[MySQL R41-3]: Rumah dimuat: %i pcs.", MAX_HOUSES);
	}

	for(new i, z = sizeof house_exit; i < z; i++)
	{
	    CreateDynamic3DTextLabel("Keluar rumah silakan, (gunakan)\n/exit",0xFFFFFFFF,house_exit[i][0],house_exit[i][1],house_exit[i][2], 10.0);
	}

	return 1;
}

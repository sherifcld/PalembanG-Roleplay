    {
        KillTimer(ElevatorBoostTimer); 
        FloorRequestedBy[ElevatorFloor] = INVALID_PLAYER_ID;
        Buka_Pintu_Lift();
        Floor_OpenDoors(ElevatorFloor);
        GetObjectPos(Obj_Elevator, x, y, z);
        Label_Elevator  = Create3DTextLabel("{CCCCCC}Tekan '{00FF00}Y{CCCCCC}' Untuk Pakai Lift", 0xCCCCCCAA, 1784.9822, -1302.0426, z - 0.9, 4.0, 0, 1);
        ElevatorState   = ELEVATOR_STATE_WAITING;
        SetTimer("Elevator_TurnToIdle", WAKTU_TUNGGU_LIFT, 0);
    }
	return 1;
}


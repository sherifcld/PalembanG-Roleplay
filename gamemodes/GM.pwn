stock SetPlayerAsClientForTaxi(playerid, vehicleid)
{
    new driver_id = vehicle_driver[vehicleid];

    if(driver_id == INVALID_PLAYER_ID) return 0;
	if(player_taxi_passanger[driver_id] != INVALID_PLAYER_ID) return 0;

    if(player_taxi_passanger[driver_id] == INVALID_PLAYER_ID)
    {

stock IsRpNickname(nickname[])
{
    new Regex:r = Regex_New("[A-Z][a-z]+_[A-Z][a-z]+");

    new check = Regex_Check(nickname, r);

    Regex_Delete(r);

    return check;
}

stock UpdatePlayerVehicleShop(playerid, bool: update_camera = false)
{
	new shop_id = GetPlayerUseListitem(playerid);

	new veh_id = player_select_veh[playerid];
	new old_veh_id = player_select_old[playerid];

	new str[32];
    new model = as_vehicle_info[shop_id][veh_id][0];
    new price = as_vehicle_info[shop_id][veh_id][1];

	PlayerTextDrawSetString(playerid, autoshop_player_td[playerid][0], VehicleNames[model-400]);

	format(str, 16, "%i$", price);
	PlayerTextDrawSetString(playerid, autoshop_player_td[playerid][1], str);

	format(str, 16, "%i", VehStats[model - 400][vMaxSpeed]);
	PlayerTextDrawSetString(playerid, autoshop_player_td[playerid][2], str);

	format(str, 16, "%.0f", VehStats[model - 400][vMaxFuel]);
	PlayerTextDrawSetString(playerid, autoshop_player_td[playerid][3], str);

	if(update_camera)
	{
	    //Cari tahu posisi mobil masa depan

	    new Float: next_x = as_vehicle_pos[veh_id][0],
            Float: next_y = as_vehicle_pos[veh_id][1];

        GetXYInFront(next_x, next_y, as_vehicle_angle, 7.0);

        //Cari tahu posisi mobil masa lalu

        new Float: old_x = as_vehicle_pos[old_veh_id][0],
            Float: old_y = as_vehicle_pos[old_veh_id][1];

        GetXYInFront(old_x, old_y, as_vehicle_angle, 7.0);

        //Membuat kamera bergerak

        new Float: from_x = old_x,
            Float: from_y = old_y,
            Float: to_x = next_x,
            Float: to_y = next_y;

        InterpolateCameraPos(playerid, from_x, from_y, as_vehicle_pos[old_veh_id][2] + 2.5, to_x, to_y, as_vehicle_pos[veh_id][2] + 2.5, 2000, CAMERA_MOVE);

		InterpolateCameraLookAt
		(
			playerid,
			as_vehicle_pos[old_veh_id][0], as_vehicle_pos[old_veh_id][1], as_vehicle_pos[old_veh_id][2],
			as_vehicle_pos[veh_id][0], as_vehicle_pos[veh_id][1], as_vehicle_pos[veh_id][2],
			2000
		);

		as_player_camera[playerid] = SetTimerEx("SetAutoShopPlayerCamera", 2000, false, "ii", playerid, veh_id);

    }
}


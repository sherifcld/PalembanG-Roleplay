stock StopGamePB(team_win)
{
	if(team_win == 0) SendClientMessageToAll(-1, "[PB] Tim biru menang.");
	else if(team_win == 1) SendClientMessageToAll(-1, "[PB] Tim Merah Memenangkan.");
	else SendClientMessageToAll(-1, "[PB] Pertandingan sudah berakhir. Gambar.");

	new count = 0;

	foreach(Player, i) if(player_reg_pb[i] == team_win) count++;

	new finish_reward = pb_reward / count;

	foreach(Player, i)
    {
        if(player_reg_pb[i] == -1) continue;

		if(player_reg_pb[i] == team_win) GivePlayerMoneyEx(i, finish_reward, "menang PB");

        ResetPlayerWeapons(i);

        SetPlayerPos(i, -2161.8418,641.5208,1052.3750);
        SetPlayerFacingAngle(i, 84.9472);
        SetPlayerVirtualWorld(i, 1);
        SetPlayerInterior(i, 1);

        player_reg_pb[i] = -1;
	}

	pb_state = pb_time = 0;

	return 1;
}


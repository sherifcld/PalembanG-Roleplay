                if(player_spawn_car_slot[playerid] != -1) return SendError(playerid, "Silakan Menyuruh Pemain Ini Untuk Membongkar Kendaraan Dia Terlebih Dahulu");

                if(PlayerInfo[playerid][pCash] < price) return SCM(playerid, COLOR_RED, "ERROR: {FFFFFF}Jumlah Uang Anda Tidak Cukup Untuk Membeli Kendaraan Ini");

                CarInfo[playerid][cModel][u_car_1] = CarInfo[offer_id][cModel][u_car_0];
                CarInfo[playerid][cSost][u_car_1] = CarInfo[offer_id][cSost][u_car_0];

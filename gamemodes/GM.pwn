stock Elevator_Initialize()
{
    Obj_Elevator                     = CreateObject(18755, 1786.678100, -1303.459472, LANTAI_PALING_BAWAH + ELEVATOR_OFFSET, 0.000000, 0.000000, 270.000000);
    Obj_ElevatorDoors[0]    = CreateObject(18757, X_DOOR_CLOSED, -1303.459472, LANTAI_PALING_BAWAH, 0.000000, 0.000000, 270.000000);
    Obj_ElevatorDoors[1]    = CreateObject(18756, X_DOOR_CLOSED, -1303.459472, LANTAI_PALING_BAWAH, 0.000000, 0.000000, 270.000000);
    Label_Elevator                 = Create3DTextLabel("{CCCCCC}Tekan '{00FF00}Y{CCCCCC}' Untuk Pakai Lift", 0xCCCCCCAA, 1784.9822, -1302.0426, 13.6491, 4.0, 0, 1);
    new string[128],
        Float:z;
    for(new i; i < sizeof(Obj_FloorDoors); i ++)
    {
        Obj_FloorDoors[i][0]    = CreateObject(18757, X_DOOR_CLOSED, -1303.171142, GetDoorsZCoordForFloor(i), 0.000000, 0.000000, 270.000000);
        Obj_FloorDoors[i][1]    = CreateObject(18756, X_DOOR_CLOSED, -1303.171142, GetDoorsZCoordForFloor(i), 0.000000, 0.000000, 270.000000);
        format(string, sizeof(string), "{CCCCCC}[%s]\n{CCCCCC}Tekan '{00FF00}Y{CCCCCC}' Untuk Memanggil Lift", NamaLantai[i]);
        if(i == 0)
            z = 13.4713;
        else
            z = 13.4713 + 8.7396 + ((i-1) * 5.45155);
        Label_Floors[i]         = Create3DTextLabel(string, 0xCCCCCCAA, 1783.9799, -1300.7660, z, 10.5, 0, 1);
    }
    Floor_OpenDoors(0);
    Buka_Pintu_Lift();
    return 1;
}
stock Elevator_Destroy()
{
    DestroyObject(Obj_Elevator);
    DestroyObject(Obj_ElevatorDoors[0]);
    DestroyObject(Obj_ElevatorDoors[1]);
    Delete3DTextLabel(Label_Elevator);
    for(new i; i < sizeof(Obj_FloorDoors); i ++)
    {
        DestroyObject(Obj_FloorDoors[i][0]);
        DestroyObject(Obj_FloorDoors[i][1]);
        Delete3DTextLabel(Label_Floors[i]);
    }
    return 1;
}
stock Buka_Pintu_Lift()
{
    new Float:x, Float:y, Float:z;
    GetObjectPos(Obj_ElevatorDoors[0], x, y, z);
    MoveObject(Obj_ElevatorDoors[0], X_DOOR_L_OPENED, y, z, KECEPATAN_PINTU_LIFT);
    MoveObject(Obj_ElevatorDoors[1], X_DOOR_R_OPENED, y, z, KECEPATAN_PINTU_LIFT);
    for(new i = 0; i < MAX_PLAYERS; i++)
          {
               if(IsPlayerInRangeOfPoint(i, 50.0, x, y, z))
               {
                     PlayAudioStreamForPlayer(i, "http://mboxdrive.com/LiftByRefki.mp3");
               }
           }
    return 1;
}
stock Tutup_Pintu_Lift()
{
    if(ElevatorState == ELEVATOR_STATE_MOVING)
        return 0;

    new Float:x, Float:y, Float:z;
    GetObjectPos(Obj_ElevatorDoors[0], x, y, z);
    MoveObject(Obj_ElevatorDoors[0], X_DOOR_CLOSED, y, z, KECEPATAN_PINTU_LIFT);
    MoveObject(Obj_ElevatorDoors[1], X_DOOR_CLOSED, y, z, KECEPATAN_PINTU_LIFT);
    for(new i = 0; i < MAX_PLAYERS; i++)
          {
               if(IsPlayerInRangeOfPoint(i, 50.0, x, y, z))
               {
                     PlayAudioStreamForPlayer(i, "http://mboxdrive.com/LiftByRefki.mp3");
               }
           }
    return 1;
}

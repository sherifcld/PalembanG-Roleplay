stock PlayerSendMessageToReport(playerid, type, text[])
{
    if(GetPVarInt(playerid,"pReportTime") > gettime()) return Send(playerid, COLOR_GREY, "Tunggu sebentar...");

    for(new i; i < MAX_REPORTS; i++)
    {
        if(report_info[i][r_id] != INVALID_PLAYER_ID) continue;

		report_info[i][r_id] = playerid;
		format(report_info[i][r_text], 144, text);
		report_info[i][r_time] = 0;
		report_info[i][r_type] = type;

		if(admin_level[playerid] == 0) SCMF(playerid, 0x66CC00FF, "[%s] %s[%i] : {ffd700}%s", type ? ("Pertanyaan") : ("Keluhan"), Name(playerid), playerid, text);

        format(format_string, 188, "[%s] %s[%i] : {ffd700}%s", type ? ("Pertanyaan") : ("Keluhan"), Name(playerid), playerid, text);

		if(type == 1) SendSupportMessage(0x66CC00FF, format_string);
		SendAdminMessage(0x66CC00FF, format_string);

		return SetPVarInt(playerid, "pReportTime", gettime() + 45);
    }

    return SCM(playerid, COLOR_GREY, "Laporan tersumbat, tunggu.");
}


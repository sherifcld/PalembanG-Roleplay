stock PayDay()
{
    new hour,minute,sec;
	gettime(hour,minute,sec);

    foreach(new i : Player)
	{
	    if(!PlayerInfo[i][pLogin]) continue;

        PlayerPlaySound(i, 17803, 0.0, 0.0, 0.0);
	    PlayerInfo[i][pZakon] ++;
        PlayerInfo[i][pLevel] += 1;
        LevelUpdate(i);
        OnPlayerUpdateAccountsPer(i, "pLevel", PlayerInfo[i][pLevel]);
		if(PlayerInfo[i][pInGameHourSeconds] < (60 * 20))
		{
			SCM(i, -1, "=======[ PERIKSA BANK ]=======");

			SCMF(i, -1," Waktu saat ini: {3399FF}%02i:%02i", hour,minute);

			SCM(i, -1, "");
			SCM(i, -1, " {a86cfc}[Perhatikan] "W"Untuk menerima gaji, Anda harus bermain setidaknya 20 menit untuk jam saat ini.");
			SCMF(i, -1, " {a86cfc}[Perhatikan] "W"Anda memenangkan segalanya %i menit (/time - Taruhan waktu per jam).", PlayerInfo[i][pInGameHourSeconds] / 60);
			SCM(i, -1, "");

            TextDrawShowForPlayer(i, TDPAYDAY[0]);
            TextDrawShowForPlayer(i, TDPAYDAY[1]);
            TextDrawShowForPlayer(i, TDPAYDAY[2]);
            TextDrawShowForPlayer(i, TDPAYDAY[3]);
            TextDrawShowForPlayer(i, TDPAYDAY[4]);
            TextDrawShowForPlayer(i, TDPAYDAY[5]);
            TextDrawShowForPlayer(i, TDPAYDAY[6]);
            TextDrawShowForPlayer(i, TDPAYDAY[7]);
            TextDrawShowForPlayer(i, TDPAYDAY[8]);
            TextDrawShowForPlayer(i, TDPAYDAY[9]);
            TextDrawShowForPlayer(i, TDPAYDAY[10]);
            TextDrawShowForPlayer(i, TDPAYDAY[11]);
            PlayerTextDrawShow(i, PAYDAYTD[i][0]);
            PlayerTextDrawShow(i, PAYDAYTD[i][1]);
            PlayerTextDrawShow(i, PAYDAYTD[i][2]);
            PlayerTextDrawShow(i, PAYDAYTD[i][3]);
            new DarryDerr[128];
            format(DarryDerr, sizeof(DarryDerr), "GAJI_:_RP.%i___SALDO_:_%d", PlayerInfo[i][pCash], PlayerInfo[i][pBank]);
            PlayerTextDrawSetString(i, PAYDAYTD[i][0], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "_");
            PlayerTextDrawSetString(i, PAYDAYTD[i][1], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "%d", PlayerInfo[i][pLevel]);
            PlayerTextDrawSetString(i, PAYDAYTD[i][2], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "%d", PlayerInfo[i][pLevel]+1);
            PlayerTextDrawSetString(i, PAYDAYTD[i][3], DarryDerr);
            PayDayOff[i] = SetTimerEx("PayDayOffline", 6000, true, "d", i);
			SCM(i, -1, "===============================");
		}
		else
		{
			Send(i, -1, "=======[ PERIKSA BANK ]=======");

			SCMF(i, -1," Waktu saat ini: {3399FF}%02i:%02i", hour,minute);

			new posobie = 50 + random(300);

			new salary = PlayerInfo[i][pPayCheck];

			if(PlayerInfo[i][pLevel] <= 3) SCMF(i, -1, " Tunjangan: {66CC00}%d$", posobie), GivePlayerBankEx(i, posobie, "uang saku");
			else SCMF(i, -1, " Pajak negara: {66CC00}%d$", posobie), GivePlayerBankEx(i, -posobie, "pajak");

			if(PlayerInfo[i][pProducts][0]) SCMF(i, -1, " Aktif saldo telepon: {66CC00}%i$", PlayerInfo[i][pSummaNumber]);

			Send(i, -1, "___________________________");
			Send(i, -1, " ");
			if(PlayerInfo[i][pVip] == 3) salary *= 2;

			SCMF(i, -1, " Gaji: {66CC00}%d$", salary);

			GivePlayerBankEx(i, salary, "gaji");

			SCMF(i, -1, " Saldo saat ini: {66CC00}%d$", PlayerInfo[i][pBank]);
   			GameTextForPlayer(i, "~w~PayDay", 5000, 1);
   			Send(i, -1, "===============================");

            TextDrawShowForPlayer(i, TDPAYDAY[0]);
            TextDrawShowForPlayer(i, TDPAYDAY[1]);
            TextDrawShowForPlayer(i, TDPAYDAY[2]);
            TextDrawShowForPlayer(i, TDPAYDAY[3]);
            TextDrawShowForPlayer(i, TDPAYDAY[4]);
            TextDrawShowForPlayer(i, TDPAYDAY[5]);
            TextDrawShowForPlayer(i, TDPAYDAY[6]);
            TextDrawShowForPlayer(i, TDPAYDAY[7]);
            TextDrawShowForPlayer(i, TDPAYDAY[8]);
            TextDrawShowForPlayer(i, TDPAYDAY[9]);
            TextDrawShowForPlayer(i, TDPAYDAY[10]);
            TextDrawShowForPlayer(i, TDPAYDAY[11]);
            PlayerTextDrawShow(i, PAYDAYTD[i][0]);
            PlayerTextDrawShow(i, PAYDAYTD[i][1]);
            PlayerTextDrawShow(i, PAYDAYTD[i][2]);
            PlayerTextDrawShow(i, PAYDAYTD[i][3]);
            new DarryDerr[128];
            format(DarryDerr, sizeof(DarryDerr), "GAJI_:_RP.%i___SALDO_:_%d", PlayerInfo[i][pCash], PlayerInfo[i][pBank]);
            PlayerTextDrawSetString(i, PAYDAYTD[i][0], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "_");
            PlayerTextDrawSetString(i, PAYDAYTD[i][1], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "%d", PlayerInfo[i][pLevel]);
            PlayerTextDrawSetString(i, PAYDAYTD[i][2], DarryDerr);

            format(DarryDerr, sizeof(DarryDerr), "%d", PlayerInfo[i][pLevel]+1);
            PlayerTextDrawSetString(i, PAYDAYTD[i][3], DarryDerr);
            PayDayOff[i] = SetTimerEx("PayDayOffline", 6000, true, "d", i);

		    new exp_amount = 1;

			if(PlayerInfo[i][pVip] == 3)
			{
			    exp_amount ++;
			    Send(i,-1,"Kamu punya {7FB151}x2{ffffff} gaji dan {7FB151}x2{ffffff} EXP karena VIP akun.");
			}
			else
			{
			    if(PlayerInfo[i][pLevel] < 2 && strlen(PlayerInfo[i][pPromo])) exp_amount ++;
			}

			if(bonus_exp) exp_amount++;

			GivePlayerExp(i, exp_amount);

			if(PlayerInfo[i][pWanted] > 0)
			{
				PlayerInfo[i][pWanted]--;
				SetPlayerWantedLevel(i, PlayerInfo[i][pWanted]);
			}

			if(PlayerInfo[i][pPayCheck] > 0) OnPlayerUpdateAccountsPer(i, "pPayCheck", 0);
			PlayerInfo[i][pPayCheck] = 0;
		}
		//
		if(GetPlayerHouse(i) != -1) { if(HouseInfo[GetPlayerHouse(i)][hOplata] <= 2) Send(i, -1, "{CC7B6C}[Perhatikan]: "W"Anda memiliki beberapa hari gaji tersisa untuk rumah."); }
		if(GetPlayerBizz(i) != -1) { if(BizzInfo[GetPlayerBizz(i)][bOplata] <= 2) Send(i, -1, "{CC7B6C}[Perhatikan]: "W"Anda memiliki beberapa hari pembayaran tersisa untuk bisnis ini."); }
        if(GetPlayerABizz(i) != 0) { if(BizzAInfo[GetPlayerABizz(i)][aOplata] <= 2) Send(i, -1, "{CC7B6C}[Perhatikan]: "W"Anda memiliki beberapa hari gaji tersisa untuk pompa bensin."); }
        GameTextForPlayer(i, "~w~PayDay", 5000, 1);

        player_green_hits[i] = 0;
		PlayerInfo[i][pInGameHourSeconds] = 0;
	}

	format(format_string, 188, "UPDATE `accounts` SET `pInGameHourSeconds` = 0, `last_hour` = %i WHERE `pLogin` = 1", hour);
	mysql_tquery(mMysql, format_string);

	return 1;
}

        PlayerInfo[playerid][pBait] -= 1;
        ShowFish(playerid);
        FishTimerTD[playerid] = SetTimerEx("HideFish", 3000, true, "d", playerid);
    }
    else if(RandIkan[playerid] == 4)
    {
        RandIkan[playerid] = 0;
        ProgresFish[playerid] = 0;
        PlayerInfo[playerid][pSampah] += 2;
        PlayerInfo[playerid][pBait] -= 2;

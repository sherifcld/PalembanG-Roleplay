				KickD(playerid, "");
			}
		}
		else
		{
			CheckPlayerAccount(playerid);

			//if(mysql_errno()) return MysqlErrorMessage(playerid);
		}
    }
    else
    {
   		CheckPlayerAccount(playerid);

		//if(mysql_errno()) return MysqlErrorMessage(playerid);
    }
	return true;
}

epublic: LoadMyWarns(playerid)
{

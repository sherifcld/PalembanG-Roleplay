CMD:free(playerid, params[])
{
    if(PlayerInfo[playerid][pMember] != FRACTION_MAYOR && PlayerInfo[playerid][pRank] != 4) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");
    if(form_fraction[playerid] == 0) return Send(playerid,COLOR_GREY,"Anda harus siap!");
    if(sscanf(params, "dd",params[0],params[1])) return Send(playerid, -1, "Masuk: /free [id] [harga (dari 5.000 sebelumnya 10.000$)]");
    if(params[1] < 5000 || params[1] > 10000) return Send(playerid, COLOR_GREY, "Harga mungkin tidak lebih rendah 5.000 dan lebih tinggi 10.000!");
	if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");//+
	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum login");//+;
	if(afk_time[params[0]] > 2) return Send(playerid, -1,"Pemain AFC ini.");//+
	if(params[0] == playerid) return Send(playerid,COLOR_GREY,"Anda tidak dapat menggunakannya untuk diri sendiri.");
	if(!ProxDetectorS(10.0, playerid, params[0])) return Send(playerid,COLOR_GREY,"Pemain harus berada di dekat Anda.");
	if(IsACop(params[0])) return Send(playerid, COLOR_GREY,"Anda tidak dapat menggunakan pada legalis.");
	if(PlayerInfo[params[0]][pPrison] < 0) return Send(playerid, COLOR_GREY, "Pemain tidak di penjara!");
	format(format_string, sizeof(format_string), "{ffffff}Pengacara %s[%i] mengundang Anda untuk dibebaskan $%i\n\n\tKamu setuju?", Name(playerid),playerid,params[1]);
	SPD(params[0], 220, DIALOG_STYLE_MSGBOX, " ",format_string, "Ya", "Tolak");
	//SendClientMessage(playerid,-1,"Anda menyarankan %s pergi gratis %i",Name(params[0]),params[1]);
	FreeOffer[params[0]] = playerid;
	FreePrice[params[0]] = params[1];
    return true;
}
CMD:prison(playerid, params[])
{
    if(admin_level[playerid] < 2) return true;
	if(sscanf(params, "dds[24]",params[0],params[1],params[2])) return Send(playerid, -1, "Masuk: /prison [id] [menit] [alasan]");
 	if(!IsPlayerConnected(params[0])) return Send(playerid, -1,"Pemain sedang offline");//+
	if(PlayerInfo[params[0]][pLogin] == 0) return Send(playerid, -1,"Pemain belum login");//+;
	if(params[1] > 300) return Send(playerid,COLOR_GREY,"Tidak dapat ditanam lebih dari 300 menit (5 jam)");

	if(PlayerCuffed[params[0]])
	{
		GameTextForPlayer(params[0],"~g~uncuffed", 5000, 3);
		if(IsPlayerAttachedObjectSlotUsed(params[0], 8)) RemovePlayerAttachedObject(params[0], 8);
		SetPlayerSpecialAction(params[0],0), PlayerCuffed[params[0]] = 0;
		PlayerCuffed[params[0]] = 0;
	}
	if(params[1] <= 0)
	{
	    //format(String, sizeof(String), "~y~<CMD> ~w~%s[%d] unjailed player %s[%d]", Name(playerid), playerid, Name(params[0]), params[0]), AdminChatMessage(String);
	    PlayerInfo[params[0]][pPrison] = 0;
	    PlayerInfo[params[0]][pMestoPrison] = 0;
	    SetPlayerHealth(params[0], 100.0);
	    return SpawnPlayerEx(params[0]);
 	}

	PlayerInfo[playerid][pAdmPrison] ++;
	OnPlayerUpdateAdminPer(playerid, "pAdmPrison", PlayerInfo[playerid][pAdmPrison]);

	SetPlayerPrison(params[0], 5, params[1]*60);

 	t_SetPlayerPos(params[0], 3518.8623,1075.6036,17.1577), SetPlayerFacingAngle(params[0], 90.7966);
 	SetPlayerVirtualWorld(params[0], 1), SetPlayerInterior(params[0], 0);
	SetCameraBehindPlayer(params[0]);

	format(format_string, sizeof(format_string), "Administrator %s[%d] menempatkan pemain %s[%d] di demorgan on %d min. Alasan: %s", Name(playerid), playerid, Name(params[0]), params[0], params[1], params[2]);
	SendClientMessageToAll(COLOR_LIGHTRED, format_string);
	AddAdminLog(playerid, format_string);
    //format(String, sizeof(String), "~y~<CMD> ~w~%s[%d] put in jail player %s[%d] for %d min", Name(playerid), playerid, Name(params[0]), params[0], params[1]), AdminChatMessage(String);
	return 1;

		new vehicleid = GetPlayerVehicleID(playerid);

		if(vehicleid != INVALID_VEHICLE_ID && (70 <= PlayerInfo[playerid][pJob] <= 72) && (70 <= VehInfo[vehicleid][vFr] <= 72) && player_taxi_passanger[playerid] != INVALID_PLAYER_ID)

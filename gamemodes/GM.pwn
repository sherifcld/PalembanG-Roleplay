stock GivePlayerExp(playerid, amount)
{
	for(new x; x < amount; x++)
	{
	    PlayerInfo[playerid][pExp]++;
	    if(PlayerInfo[playerid][pExp] >= (PlayerInfo[playerid][pLevel] + 1) * 4)
		{
		    PlayerInfo[playerid][pLevel] ++;
			PlayerInfo[playerid][pExp] = 0;
			LevelUpdate(playerid);
			Send(playerid, COLOR_ISPOLZUY, "Selamat! Level game Anda telah ditingkatkan...");

            if(PlayerInfo[playerid][pLevel] == 4 && PlayerInfo[playerid][pReferal])
			{
			    new ref_id = PlayerInfo[playerid][pReferal];
			    new to_player = GetPlayerID(GetPlayerNameBySqlID(ref_id));

				if(to_player != INVALID_PLAYER_ID)
				{
				    SCMF(to_player, COLOR_YELLOW, "[Referal] {FFFFFF}Kamu punya $50.000 untuk undangan %s", GetPlayerNameEx(playerid));
				    GivePlayerMoneyEx(to_player, 50000, "rujukan");
				}

				else
				{
				    format(format_string, 144, "UPDATE `accounts` SET `pCash` = `pCash` + 50000 WHERE `pID` = %i", ref_id);
				    mysql_tquery(mMysql, format_string);
				}
			}

			if(PlayerInfo[playerid][pLevel] == 4 && strlen(PlayerInfo[playerid][pPromo]))
			{
				format(format_string, 128, "SELECT * FROM `promocodes` WHERE `code` = '%s'", PlayerInfo[playerid][pPromo]);
				new Cache: result = mysql_query(mMysql, format_string, true);


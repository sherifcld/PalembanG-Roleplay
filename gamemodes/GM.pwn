				    mysql_format(mMysql, MySQLStr, 144, "UPDATE `accounts` SET `pMail` = '%e' WHERE `pName` = '%e'",PlayerInfo[playerid][pMail], Name(playerid)), mysql_tquery(mMysql, MySQLStr, "", "");


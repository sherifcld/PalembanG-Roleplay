        SCMF(playerid, 0xffff00ff, "Anda ditugaskan %s(%i) dukungan", Name(params[0]), params[0]);

		format(String, 244, "INSERT INTO `supports` (`user_id`, `offer`) VALUES (%i, '%s')", GetPlayerAccountID(params[0]), Name(playerid));
		mysql_tquery(mMysql, String);

		Iter_Add(support_list, params[0]);
	}
	else
	{
	    SCMF(params[0], 0xffff00ff, "Administrator %s(%i) menarik hak dukungan dari Anda", Name(playerid), playerid);
        SCMF(playerid, 0xffff00ff, "Anda mengambil hak dukungan dari %s(%i)", Name(params[0]), params[0]);


CMD:sellallfish(playerid)
{
    if(!IsPlayerInRangeOfPoint(playerid, 3.5, 53.4075,-1601.4459,14.5446))
        return ShowError(playerid, "Kamu Tidak Berada Di Market Kota!");
    
    if(PlayerInfo[playerid][pIkan] < 1) return ShowError(playerid, "Anda tidak memiliki ikan");
    new pay = PlayerInfo[playerid][pIkan] * 8000;
    new total = PlayerInfo[playerid][pIkan];
    GivePlayerMoneyEx(playerid, pay, "HargaIkan");

    KirimPesan(playerid, -1, "You selling "RED_E"%d kg "WHITE_E"fish to "GREEN_E"Rp.%d", total, pay);
    return 1;
}


stock StartPlayerRob(playerid, partnerid = INVALID_PLAYER_ID)
{
	for(new i; i < MAX_ROBBS; i++)
	{
	    if(rob_info[i][r_player][0] != INVALID_PLAYER_ID) continue;

	    rob_info[i][r_player][0] = playerid;
	    rob_info[i][r_player][1] = partnerid;

	    rob_info[i][r_house] = -1;
	    rob_info[i][r_loaded] = 0;

	    player_rob_id[playerid] = i;
	    if(partnerid != INVALID_PLAYER_ID) player_rob_id[partnerid] = i;

	    SendMessageToRobPlayers(i, 0x6495EDff, "[ROB]: Anda telah memulai tahap persiapan.");
	    SendMessageToRobPlayers(i, 0x6495EDff, "[ROB]: Berangkat pada {ff0000}merah {6495ED}memberi label dan mengambil transportasi.");

	    //

		new rand = rob_vehicle_number;

		new Float: x = rob_vehicle_pos[rand][0],
		    Float: y = rob_vehicle_pos[rand][1],
		    Float: z = rob_vehicle_pos[rand][2];

	    SetPlayerGPS(playerid, x, y, z);
	    if(partnerid != INVALID_PLAYER_ID) SetPlayerGPS(partnerid, x, y, z);

	    new vehicleid = AddStaticVehicleEx(413, x, y, z, rob_vehicle_pos[rand][3], random(128), random(128), 9999);

        GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
        SetVehicleParamsEx(vehicleid,engine,lights,alarm,0,bonnet,boot,objective);

	    VehInfo[vehicleid][vStatus] = 2;
	    Fuell[vehicleid] = 50;

	    rob_info[i][r_vehicleid] = vehicleid;

	    rob_vehicle_number++;

	    if(rob_vehicle_number == sizeof rob_vehicle_pos) rob_vehicle_number = 0;

	    return 1;
	}

	return SCM(playerid, COLOR_GREY, "Batas perampokan simultan tercapai.");
}


	}

    for(new i; i < 5; i++) VehInfo[vehicleid][vEngineTune][i] = VehInfo[vehicleid][vBrakeTune][i] = 0;

    ClearVehicleData(vehicleid);

    if(VehInfo[vehicleid][vFr] == 4)
    {
        trash_vehicle_owner[vehicleid] = INVALID_PLAYER_ID;
        trash_vehicle_amount[vehicleid] = 0;

		if(_:trash_vehicle_3d[vehicleid] != 0)
		{
		    DestroyDynamic3DTextLabel(trash_vehicle_3d[vehicleid]);
			trash_vehicle_3d[vehicleid] = Text3D:0;
		}
	}
	if(VehInfo[vehicleid][vFr] == 5)
    {
        collector_vehicle_owner[vehicleid] = INVALID_PLAYER_ID;
        collector_vehicle_amount[vehicleid] = 0;
	}

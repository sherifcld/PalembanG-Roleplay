stock SetPlayerSchoolPoint(playerid)
{
	new road_id = player_road_id[playerid];

	new next_id = (road_id + 1) == sizeof school_road ? sizeof school_road - 1 : road_id + 1;

	SetPlayerRaceCheckpoint(playerid,
		road_id != next_id ? 0 : 1,

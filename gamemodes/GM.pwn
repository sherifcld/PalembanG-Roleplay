stock GetPlayerHouseLodger(playerid)
{
	for(new i; i < MAX_HOUSES; i++)
	{
	    if(!HouseInfo[i][hOwned]) continue;

	    for(new x; x < MAX_HOUSE_LODGERS; x++) if(!strcmp(house_lodgers[i][x], Name(playerid), true)) return i;
 	}

 	return -1;
}

stock GetPlayerHouse(playerid)
{
	for(new i; i < MAX_HOUSES; i++)
	{
	    if(HouseInfo[i][hOwned] && !strcmp(HouseInfo[i][hOwner],Name(playerid),true)) return i;
 	}

 	return -1;
}
stock GetPlayerBizz(playerid)
{
	for(new i; i < TotalBizz; i++) if(BizzInfo[i][bOwned] && !strcmp(BizzInfo[i][bOwner],Name(playerid),true)) return i;

 	return -1;
}
stock GetPlayerABizz(playerid)
{
	new abizz = 0;
	for(new i = 1; i <= TotalABizz; i++)
	{
	    if(BizzAInfo[i][aOwned] && !strcmp(BizzAInfo[i][aOwner],Name(playerid),true)) abizz = i;
 	}
 	return abizz;
}


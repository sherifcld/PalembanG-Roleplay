stock DayOfWeak()
{
    switch(((gettime()+3*3600)/86400)%7)
    {
        case 0: return 4;
        case 1: return 5;
        case 2: return 6;
        case 3: return 7;
        case 4: return 1;
        case 5: return 2;
        case 6: return 3;
    }
    return -1;
}


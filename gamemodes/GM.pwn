	}

	if(GetPlayerHouse(playerid) != -1)
	{
	    new h_id = GetPlayerHouse(playerid);

	    format(HouseInfo[h_id][hOwner], MAX_PLAYER_NAME, new_nick);

	    format(format_string, 144, "UPDATE `house` SET `hOwned` = '%s' WHERE `hID` = %i", new_nick, h_id);
	    mysql_tquery(mMysql, format_string);
	}

    if(PlayerInfo[playerid][pHotel] != -1)
	{
	    new h_id = PlayerInfo[playerid][pHotel];

        format(hotel_info[h_id][hOwner], MAX_PLAYER_NAME, new_nick);

	    format(format_string, 144, "UPDATE `hotels` SET `hOwner` = '%s' WHERE `id` = %i", new_nick, h_id);
	    mysql_tquery(mMysql, format_string);
	}

	if(GetPlayerBizz(playerid) != -1)
	{
	    new b_id = GetPlayerBizz(playerid);

        format(BizzInfo[b_id][bOwner], MAX_PLAYER_NAME, new_nick);

	    format(format_string, 144, "UPDATE `bizz` SET `bOwned` = '%s' WHERE `bID` = %i", new_nick, b_id);
	    mysql_tquery(mMysql, format_string);
	}

	if(GetPlayerABizz(playerid) != 0)
	{
	    new a_id = GetPlayerABizz(playerid);

        format(BizzAInfo[a_id][aOwner], MAX_PLAYER_NAME, new_nick);

	    format(format_string, 144, "UPDATE `bizz_fuel` SET `aOwned` = '%s' WHERE `aID` = %i", new_nick, a_id);
	    mysql_tquery(mMysql, format_string);
	}

	if(PlayerInfo[playerid][pFamily] != -1)
	{
	    new f_id = PlayerInfo[playerid][pFamily];

	    if(strfind(GetPlayerNameEx(playerid), family_info[f_id][f_owner], true) != -1)

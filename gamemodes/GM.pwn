	        if(!BizzAInfo[i][aOwned]) continue;

            if(BizzAInfo[i][aOplata]) BizzAInfo[i][aOplata] --;
            if(BizzAInfo[i][aOplata] <= 0) BizzAInfo[i][aOplata] = 0, BizzAInfo[i][aSlet] = 1;

    		format(MySQLStr, 144, "INSERT INTO `bizz_astats`(`aID`, `aData`, `aSumma`) VALUES (%d,'%s',%d)", i, strstre, BizzAInfo[i][aRaschet]);
			mysql_tquery(mMysql, MySQLStr, "", "");

			BizzAInfo[i][aRaschet] = 0;
			SaveABizz(i);
		}
  	}
  	if(hour == 00 && minute == 00 && second == 30)
 	{
 	    for(new i; i < MAX_HOUSES; i++)
		{
		    if(HouseInfo[i][hOwned] == 1)
		    {
				if(HouseInfo[i][hOplata] >= 1) HouseInfo[i][hOplata] --;
				if(HouseInfo[i][hOplata] <= 0) HouseInfo[i][hOplata] = 0, HouseInfo[i][hSlet] = 1;
				SaveHouse(i);
			}
  	   	}
  	}
  	for(new v = 0; v < MAX_VEHICLES; v++)
	{
	    if(pizza_veh_time{v})
	    {
	        pizza_veh_time{v}--;
	        if(pizza_veh_time{v} == 0) pizza_veh_have{v} = 0;
	    }

		GetVehicleParamsEx(v,engine,lights,alarm,doors,bonnet,boot,objective);
		if(engine && Fuell[v] > 0.0 && IsVehicleHaveFuel(v))
		{
			if(GetVehicleSpeed(v) < 1.0) Fuell[v] -= 0.01;
			else if(1.0 <= GetVehicleSpeed(v) <= 60.0) Fuell[v] -= 0.02;
		    else Fuell[v] -= 0.03;

			if(vehicle_owner_id[v] != INVALID_PLAYER_ID)
			{
			    new owner_id = vehicle_owner_id[v];

			    for(new i; i < MAX_PLAYER_VEHICLES; i++)
			    {
			        if(v != player_home_car[owner_id][i]) continue;

                    CarInfo[owner_id][cFuel][i] = Fuell[v];
					CarInfo[owner_id][cProbeg][i] = Milliage[v];
			    }
			}
		}
	}

	new Float: health;

	for(new i; i < MAX_VEHICLES; i++)
	{
		GetVehicleHealth(i, health);
		if(health < 350.0)
		{
			SetVehicleHealth(i, 350.0);

			GetVehicleParamsEx(i,engine,lights,alarm,doors,bonnet,boot,objective);
    		return SetVehicleParamsEx(i, 0,lights,alarm,doors,bonnet,boot,objective);
		}
	}

	return 1;
}
stock PlayerUpdate()
{
    new hour, minute, second,stateEx;
	gettime(hour, minute, second);

	if(hour == 23 && minute == 59 && second == 59)
	{
	    foreach(Player, i) PlayerInfo[i][pInGameSeconds] = PlayerInfo[i][pInGameAFKSeconds] = 0;

		format(format_string, 188, "UPDATE `accounts` SET `pInGameSeconds` = 0, `pInGameAFKSeconds` = 0, `last_curdate` = %i WHERE `pLogin` = 1", getdate());
		mysql_tquery(mMysql, format_string);
	}

	foreach(Player, playerid)
	{
	    stateEx = GetPlayerState(playerid);

		if(GetPVarInt(playerid, "LoginOgran") != -1)
		{
			SetPVarInt(playerid, "LoginOgran", GetPVarInt(playerid, "LoginOgran") -1);

			if(GetPVarInt(playerid, "LoginOgran") == 0) return KickD(playerid, "Anda telah lama ditendang dalam mode otorisasi / registrasi!");
		}

		if(cant_make_offer[playerid]) cant_make_offer[playerid]--;

		if(GetPVarInt(playerid,"Drug_Time"))
		{
			SetPVarInt(playerid,"Drug_Time",GetPVarInt(playerid,"Drug_Time") - 1);
			if(GetPVarInt(playerid,"Drug_Time") == 100) SetPlayerWeather(playerid, weather_id);
		}

		if((beg_quest_info[playerid][b_quest_id] == QUEST_TYPE_DRIVER && PlayerInfo[playerid][pLicenses][0] ||
			beg_quest_info[playerid][b_quest_id] == QUEST_TYPE_PHONE && PlayerInfo[playerid][pProducts][0] ||
			beg_quest_info[playerid][b_quest_id] == QUEST_TYPE_FRACTION && PlayerInfo[playerid][pMember]) && beg_quest_info[playerid][b_quest_status] == 1)
		{
	        SCM(playerid, COLOR_YELLOW, "[QUEST] {FFFFFF}Quest quest telah selesai, cukup serahkan.");

	        beg_quest_info[playerid][b_quest_status] = 2;

	        OnPlayerUpdateAccountsPer(playerid, "b_quest_status", 2);
		}

		if(follow_id[playerid] != INVALID_PLAYER_ID)
		{
			new to_player = follow_id[playerid];

		    if(!IsPlayerInAnyVehicle(to_player) && !IsPlayerInAnyVehicle(playerid))
			{
				if(GetDistanceBetweenPlayers(playerid, to_player) < 1.5) ClearAnimations(playerid), TogglePlayerControllable(playerid, false);
				else if(1.5 <= GetDistanceBetweenPlayers(playerid, to_player) <= 10.0)
				{
				    TogglePlayerControllable(playerid,true);
					SetPlayerToFacePlayer(playerid, to_player);
					ApplyAnimation(playerid, "ped", "WALK_civi", 6.0, 1, 1, 1, 1, 0, 1);
				}
				else if(GetDistanceBetweenPlayers(playerid, to_player) > 10.0)
				{
					new Float: x, Float: y, Float: z;

					GetPlayerPos(to_player, x, y, z);

				    SetPPos(playerid, x, y + 0.3, z, 0.0, GetPlayerInterior(playerid), GetPlayerVirtualWorld(playerid), false);

                    SetPlayerToFacePlayer(playerid, to_player);

					/*SCMF(to_player,COLOR_REDD,"Anda telah pindah terlalu jauh dari %s[%i], yang Anda pimpin!",GetPlayerNameEx(to_player),to_player);
					follow_id[playerid] = INVALID_PLAYER_ID;
					TogglePlayerControllable(playerid,false);*/
				}
		  	}
	        else
			{
				follow_id[playerid] = INVALID_PLAYER_ID;
				ClearAnimations(playerid);
			}
		}

		if(gSpectateID[playerid] != INVALID_PLAYER_ID)
		{
		    new spec_id = gSpectateID[playerid];

		    if(IsPlayerConnected(spec_id))
		    {
			    if(GetPlayerInterior(playerid) != GetPlayerInterior(spec_id)) SetPlayerInterior(playerid, GetPlayerInterior(spec_id));
			    if(GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(spec_id)) SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(spec_id));
			}
			else StopSpectate(playerid);
		}

        if(PlayerInfo[playerid][TimeAcceptdeath] != 0)
        {
            PlayerInfo[playerid][TimeAcceptdeath]--;

            new DarryDerr[128];
            format(DarryDerr, sizeof DarryDerr, "%i DETIK", PlayerInfo[playerid][TimeAcceptdeath]);
            PlayerTextDrawSetString(playerid, DEATHTD[playerid][9], DarryDerr);
        }
		if(pTemp[playerid][uborka_timer] != 0)
		{
		    pTemp[playerid][uborka_timer]--;

		    format(format_string, 16, "~r~%i", pTemp[playerid][uborka_timer]);
		    GameTextForPlayer(playerid, format_string, 1100, 4);

		    if(pTemp[playerid][uborka_timer] == 0) spawned_uborka(playerid);
		}

		if(pTemp[playerid][bus_timer] != 0)
		{
		    pTemp[playerid][bus_timer]--;

		    format(format_string, 16, "~r~%i", pTemp[playerid][bus_timer]);
		    GameTextForPlayer(playerid, format_string, 1100, 4);

		    if(pTemp[playerid][bus_timer] == 0) spawned_bus(playerid);
		}

		if(player_rob_id[playerid] != -1 && player_rob_progress[playerid] > 0)
		{
		    new rob_id = player_rob_id[playerid];

	     	if(player_rob_progress[playerid] == 1)

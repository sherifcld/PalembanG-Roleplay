{
	new member = PlayerInfo[playerid][pMember];

	if(fraction_steal_block != 0) return SCMF(playerid, COLOR_GREY, "Akan tersedia melalui %i detik.", fraction_steal_block);

	if(member != 7) return SendClientMessage(playerid, COLOR_GREY, "Anda tidak dapat menggunakannya.");

	if(PlayerInfo[playerid][pRank] < FracInfo[PlayerInfo[playerid][pMember]][fRankMax]-1) return Send(playerid,COLOR_GREY,"Anda bukan pemimpin/wakil pemimpin");// return SCMF(playerid, COLOR_GREY, "Tersedia %i+ jajaran.", FracInfo[member][fRankMax] - 2);

	new players = 0;

	foreach(Player, i) if(PlayerInfo[i][pMember] == member) players++;

	if(players < 5) return SendClientMessage(playerid, COLOR_GREY, "Dalam sepersekian kurang dari 5 orang.");

	new try_id = 0;

	SEARCH_ABIZZ:

	new fuel_id = random(TotalABizz);

	try_id++;

	if(try_id == 1000) return SCM(playerid, COLOR_GREY, "Tidak dapat menemukan pom bensin.");

	if(BizzAInfo[fuel_id][aGangAttack] != 0) goto SEARCH_ABIZZ;

	format(format_string, 144, "Klub Biker menyerang pompa bensin #%s[%i]. Gunakan: /azspos untuk tanda", BizzAInfo[fuel_id][aMessage], fuel_id);
	SendFrac(member, 0x42B66FFF, format_string);
    SendFrac(member, 0x42B66FFF, "Apakah kamu punya {FFFFFF}dua{42B66F} menit untuk persiapan.");

	BizzAInfo[fuel_id][aGangAttack] = member;
	BizzAInfo[fuel_id][aGangAttackState] = 1;
	BizzAInfo[fuel_id][aGangAttackTime] = 120;
	fraction_steal_block = 7200;

	return SetTimerEx("CheckFuelAttack", 1000, false, "i", fuel_id);
}

epublic: CheckFuelAttack(fuel_id)
{
	new gang_attack = BizzAInfo[fuel_id][aGangAttack];

	if(BizzAInfo[fuel_id][aGangAttackTime] != 0) BizzAInfo[fuel_id][aGangAttackTime]--;

	//format(format_string, 96, "%i %i %i %i", fuel_id, BizzAInfo[fuel_id][aGangAttackState], BizzAInfo[fuel_id][aGangAttack], BizzAInfo[fuel_id][aGangAttackTime]);
	//SendClientMessageToAll(-1, format_string);

	if(BizzAInfo[fuel_id][aGangAttackState] == 1)
	{
	    new string[48];
	    format(string, 48, "TIME: %i", BizzAInfo[fuel_id][aGangAttackTime]);
		foreach(Player, i) if(PlayerInfo[i][pMember] == gang_attack) GameTextForPlayer(i, string, 1500, 4);

		if(BizzAInfo[fuel_id][aGangAttackTime] == 0)
		{
		    new players = 0;

			foreach(Player, i) if(PlayerInfo[i][pMember] == gang_attack && GetPlayerDistanceFromPoint(i, BizzAInfo[fuel_id][aAreaFillX], BizzAInfo[fuel_id][aAreaFillY], BizzAInfo[fuel_id][aAreaFillZ]) < 60.0) players++;

			if(players < 1)
			{
			    BizzAInfo[fuel_id][aGangAttack] = 0;

				format(format_string, 144, "Serangan di pompa bensin dihentikan, tidak ada yang tiba di wilayahnya.");
			    return SendFrac(gang_attack, 0x42B66FFF, format_string);
			}

			BizzAInfo[fuel_id][aGangAttackState] = 2;
		}
	}
	else if(BizzAInfo[fuel_id][aGangAttackState] == 2)
	{
	    new players = 0;

		foreach(Player, i) if(PlayerInfo[i][pMember] == gang_attack && GetPlayerDistanceFromPoint(i, BizzAInfo[fuel_id][aAreaFillX], BizzAInfo[fuel_id][aAreaFillY], BizzAInfo[fuel_id][aAreaFillZ]) < 60.0) players++;

		if(players == 0)
		{
		    BizzAInfo[fuel_id][aGangAttack] = 0;

			format(format_string, 144, "Semua anggota geng meninggalkan pompa bensin, serangan berhenti.");
		    return SendFrac(gang_attack, 0x42B66FFF, format_string);
		}
	}

    return SetTimerEx("CheckFuelAttack", 1000, false, "i", fuel_id);
}

CMD:bizwar(playerid, params[])
{
	if(!IsAMafia(playerid)) return SCM(playerid, COLOR_GREY, "Anda tidak dapat menggunakan perintah ini.");

    if(PlayerInfo[playerid][pRank] < FracInfo[PlayerInfo[playerid][pMember]][fRankMax]-2) return SCM(playerid, COLOR_GREY, "Peringkat Anda kecil.");

    if(!strlen(params))
	{
		Send(playerid, -1, "/bizwar [place(0-2]");
		Send(playerid, COLOR_GREY, "0 - Desa terbengkalai");
		Send(playerid, COLOR_GREY, "1 - Reruntuhan");
		Send(playerid, COLOR_GREY, "2 - Pelabuhan");
		return 1;
	}

	new place_id = strval(params);

	new attack_id = PlayerInfo[playerid][pMember];

	if(BW_INFO[bw_attack] != 0) return SCM(playerid, COLOR_GREY, "Perang untuk bisnis sudah dimulai.");

	for(new i; i < MAX_BIZZ; i++)
	{
		if(!PlayerToPoint(15.0, playerid, BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ])) continue;

		new owner_id = BizzInfo[i][bMafia];

		if(owner_id == 0)
		{
		    BizzInfo[i][bMafia] = attack_id;

            SaveBizzPer(i, "bMafia", attack_id);

            UpdateBizzLabelText(i);

			return SCM(playerid, COLOR_BLUE, "Bisnis itu tidak memiliki atap, Anda mengendalikannya.");
		}

		if(owner_id == attack_id) return SCM(playerid, COLOR_GREY, "Anda sudah menutupi bisnis ini.");

		BW_INFO[bw_attack] = attack_id;
		BW_INFO[bw_owner] = owner_id;
		BW_INFO[bw_time] = 720;
		BW_INFO[bw_place] = place_id;
		BW_INFO[bw_bizz] = i;
		BW_INFO[bw_doubledraw] = 0;
		for(new x; x < 2; x++) BW_INFO[bw_kills][x] = 0;

		BW_INFO[bw_zone] = GangZoneCreate(bw_place_pos[place_id][0],bw_place_pos[place_id][1],bw_place_pos[place_id][2],bw_place_pos[place_id][3]);

        GangZoneShowForAll(BW_INFO[bw_zone], TEAM_HIT_COLOR);

        CreateBizWarTD();

        TextDrawSetString(BW_INFO[bw_td][3], FracInfo[owner_id][fName]);
        TextDrawSetString(BW_INFO[bw_td][4], FracInfo[attack_id][fName]);

		foreach(Player, x)
		{
			if(PlayerInfo[x][pMember] != attack_id && PlayerInfo[x][pMember] != owner_id) continue;

			for(new j; j < 7; j++) TextDrawShowForPlayer(x, BW_INFO[bw_td][j]);
		}

		SetTimer("checkBW", 1000, false);

        format(format_string, 96, "Anda menyerang bisnis %s[%i], layak %s", BizzInfo[i][bMessage], i, FracInfo[owner_id][fName]);
		SendFrac(attack_id, 0x4682B4AA, format_string);

		format(format_string, 96, "[Perhatian]: Untuk bisnis Anda %s[%i] mafia menyerang %s", BizzInfo[i][bMessage], i, FracInfo[attack_id][fName]);
		SendFrac(owner_id, 0x4682B4AA, format_string);

		format(format_string, 96, "%s memprovokasi perang untuk bisnis.", GN(playerid));
		SendFrac(attack_id, 0x4682B4AA, format_string);
		SendFrac(owner_id, 0x4682B4AA, format_string);

		format_string = "[BIZWAR] Ada zona di peta, pergi ke sana.";
		SendFrac(attack_id, -1, format_string);
		return SendFrac(owner_id, -1, format_string);
	}

	return SCM(playerid, COLOR_GREY, "Anda jauh dari memasuki bisnis apa pun.");
}

epublic: checkBW()
{
    BW_INFO[bw_time]--;

    GangZoneFlashForAll(BW_INFO[bw_zone], COLOR_RED);

	new place_id = BW_INFO[bw_place];
	new b_id = BW_INFO[bw_bizz];

    new gang_owner = BW_INFO[bw_owner];
	new gang_attack = BW_INFO[bw_attack];

	new str[16];

	format(str, 16, "~n~%s", Convert(BW_INFO[bw_time]));
	TextDrawSetString(BW_INFO[bw_td][2], str);

    new members[2];

    foreach(Player, i)
    {
        if(!PlayerToKvadrat(i,bw_place_pos[place_id][0],bw_place_pos[place_id][1],bw_place_pos[place_id][2],bw_place_pos[place_id][3])) continue;

        if(PlayerInfo[i][pMember] == gang_owner) members[0]++;
        if(PlayerInfo[i][pMember] == gang_attack) members[1]++;
    }

	TextDrawSetString(BW_INFO[bw_td][5], IntToString(members[0]));
	TextDrawSetString(BW_INFO[bw_td][6], IntToString(members[1]));

	if(BW_INFO[bw_time] == 0)
	{
        if(members[0] > members[1]) StopBizWar(1);
        else if(members[0] < members[1]) StopBizWar(2);
        else StopBizWar(0);

	    return printf("Perang untuk bisnis %s[%i] berhenti.", BizzInfo[b_id][bMessage], b_id);
	}

	return SetTimer("checkBW", 1000, false);
}

stock StopBizWar(winner)
{
    new b_id = BW_INFO[bw_bizz];

    new gang_owner = BW_INFO[bw_owner];
	new gang_attack = BW_INFO[bw_attack];

	new winner_id = winner == 2 ? gang_attack : winner == 1 ? gang_owner : 0;

	printf("[BW] %i %i", winner, winner_id);

	if(winner == 1)
	{
	    format(format_string, 96, "Mafia Anda telah melindungi bisnis dari mafia %s", FracInfo[gang_attack][fName]);
		SendFrac(gang_owner, 0x4682B4AA, format_string);

		format(format_string, 96, "Anda tidak dapat menangkap bisnis mafia %s", FracInfo[gang_owner][fName]);
		SendFrac(gang_attack, 0x4682B4AA, format_string);
	}
	else if(winner == 2)
	{
	    format(format_string, 96, "Mafia Anda telah kehilangan bisnis mafia %s", FracInfo[gang_attack][fName]);
		SendFrac(gang_owner, 0x4682B4AA, format_string);

		format(format_string, 96, "Mafia Anda mengambil bisnis dari mafia %s", FracInfo[gang_owner][fName]);
		SendFrac(gang_attack, 0x4682B4AA, format_string);

		BizzInfo[b_id][bMafia] = gang_attack;

        UpdateBizzLabelText(b_id);

        SaveBizzPer(b_id, "bMafia", gang_attack);
	}
	else
	{
	    if(BW_INFO[bw_doubledraw] == 0)
	    {
		    format_string = "Diterbitkan ekstra waktu dalam jumlah 2 menit untuk perang, saat ini skornya sama.";
			SendFrac(gang_owner, 0x4682B4AA, format_string);
			SendFrac(gang_attack, 0x4682B4AA, format_string);

			BW_INFO[bw_time] = 120;
			BW_INFO[bw_doubledraw] = 1;

			return SetTimer("checkBW", 1000, false);
		}
		else
		{
		    format_string = "Tambah. waktu habis, skor tetap sama, wilayah tetap dengan pertahanan.";
			SendFrac(gang_owner, 0x4682B4AA, format_string);
			SendFrac(gang_attack, 0x4682B4AA, format_string);
		}
	}

	foreach(Player, x)
	{
		if(PlayerInfo[x][pMember] != gang_owner && PlayerInfo[x][pMember] != gang_attack) continue;

		for(new j; j < 7; j++) TextDrawHideForPlayer(x, BW_INFO[bw_td][j]);
	}

	for(new j; j < 7; j++) TextDrawDestroy(BW_INFO[bw_td][j]);

	BW_INFO[bw_attack] = 0;

	GangZoneDestroy(BW_INFO[bw_zone]);

	return 1;
}

CMD:tcapture(playerid, params[])
{
	for(new i; i < MAX_ZONES; i++)
	{
		if(!PlayerToKvadrat(playerid,GZInfo[i][gPos][0], GZInfo[i][gPos][1],GZInfo[i][gPos][2],GZInfo[i][gPos][3])) continue;

		SCMF(playerid, -1, "%i", i);

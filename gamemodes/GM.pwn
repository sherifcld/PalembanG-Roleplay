CMD:parkveh(playerid, params[])
{
    new id = -1;
    id = GetClosestParks(playerid);

    if(!IsPlayerInAnyVehicle(playerid)) return ShowError(playerid, "You must be in Vehicle");
    if(id == -1) return ShowError(playerid, "Garkot tidak ditemukan.");

    if(id > -1)
    {
        new vehicleid = GetPlayerVehicleID(playerid);
        for(new x; x < MAX_PLAYER_VEHICLES; x++)
        {
            if(vehicleid == player_home_car[playerid][x])
            {
                player_car_park[playerid][x][0] = ppData[id][parkX];
                player_car_park[playerid][x][1] = ppData[id][parkY];
                player_car_park[playerid][x][2] = ppData[id][parkZ];
                player_car_park[playerid][x][3] = ppData[id][parkA];
                SaveCars(playerid);
                SavePlayerCarPark(playerid);

                DestroyVehicleEx(player_home_car[playerid][x]);
                player_home_car[playerid][x] = INVALID_VEHICLE_ID;
                PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
                ShowInfo(playerid, "Vehicle Despawned");
            }
        }
    }
    return 1;
}

CMD:pickupveh(playerid, params[])
{
    new id = -1;
    id = GetClosestParks(playerid);

    if(id == -1) return ShowError(playerid, "Garkot tidak ditemukan.");

    if(id > -1)
    {
        if(CarInfo[playerid][cModel][0] == 0 && CarInfo[playerid][cModel][1] == 0 && CarInfo[playerid][cModel][2] == 0 && CarInfo[playerid][cModel][3] == 0 && CarInfo[playerid][cModel][4] == 0 && CarInfo[playerid][cModel][5] == 0 && CarInfo[playerid][cModel][6] == 0) return ShowError(playerid, "Anda tidak memiliki kendaraan.");
        String = "";

        new str[96];

        new count = 0;

        for(new i; i < MAX_PLAYER_VEHICLES; i++)
        {
            if(CarInfo[playerid][cModel][i] == 0) continue;

            format(str, 96, "{a86cfc}%i.\t{ffffff}%s\t%s\n",

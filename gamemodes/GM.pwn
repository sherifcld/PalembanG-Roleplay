			SPD(playerid, 9471, DIALOG_STYLE_MSGBOX, "Pertemuan pertama", String, "Jawab", "Batalkan");
		}
		else if(q_status == 0) SPD(playerid, 9471, DIALOG_STYLE_MSGBOX, "Ujang", ""W"Saya punya tugas baru untuk Anda", "Ya", "Tidak");
